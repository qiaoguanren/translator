<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="cartridge_anhydrous potassium carbonate"
      type="cartridge"
      chemical="anhydrous potassium carbonate" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="95 % ethyl alcohol" />
    <Reagent
      id="freshly distilled acetaldehyde" />
    <Reagent
      id="granulated anhydrous calcium chloride" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="95 % ethyl alcohol"
      vessel="reactor"
      volume="1.305 l" />
    <Add
      reagent="granulated anhydrous calcium chloride"
      vessel="reactor"
      mass="200 g"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="5.0"
      stir="True" />
    <Add
      reagent="freshly distilled acetaldehyde"
      vessel="reactor"
      volume="620 mL"
      dispense_speed="10.0"
      stir="True" />
    <Stir
      vessel="reactor"
      time="36 hrs"
      stir_speed="600.0" />
    <Separate
      purpose="extract"
      from_vessel="reactor"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="reactor"
      product_bottom="False"
      solvent="water"
      through="anhydrous potassium carbonate"
      solvent_volume="330 mL"
      n_separations="3" />
    <Distill />
  </Procedure>

</Synthesis>
