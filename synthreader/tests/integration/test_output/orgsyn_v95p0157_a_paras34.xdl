<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="buffer_flask1"
      type="flask" />
    <Component
      id="cartridge_Na2SO4"
      type="cartridge"
      chemical="Na2SO4" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="CH2Cl2" />
    <Reagent
      id="Et3N" />
    <Reagent
      id="compound 2" />
    <Reagent
      id="dichloromethane" />
    <Reagent
      id="methanesulfonyl chloride" />
    <Reagent
      id="saturated aqueous NH4Cl" />
    <Reagent
      id="saturated aqueous NaCl" />
    <Reagent
      id="saturated aqueous NaHCO3" />
  </Reagents>

  <Procedure>
    <Evaporate
      rotavap_name="rotavap"
      temp="40.0"
      pressure="313.30670000000003"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="compound 2"
      vessel="rotavap"
      mass="14.7 g" />
    <Add
      reagent="CH2Cl2"
      vessel="rotavap"
      volume="175 mL" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="reactor"
      volume="all" />
    <Add
      reagent="CH2Cl2"
      vessel="rotavap"
      volume="10 mL" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="reactor"
      volume="10 mL" />
    <HeatChillToTemp
      vessel="reactor"
      temp="4.0"
      stir="True" />
    <Add
      reagent="Et3N"
      vessel="reactor"
      volume="16.6 mL"
      time="15 mins"
      stir="True" />
    <Add
      reagent="CH2Cl2"
      vessel="reactor"
      volume="5 mL"
      stir="True" />
    <Add
      reagent="methanesulfonyl chloride"
      vessel="reactor"
      volume="7.4 mL"
      time="20 mins"
      stir="True" />
    <Add
      reagent="CH2Cl2"
      vessel="reactor"
      volume="5 mL"
      stir="True" />
    <Stir
      vessel="reactor"
      time="15 mins" />
