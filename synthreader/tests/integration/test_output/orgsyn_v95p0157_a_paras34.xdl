      vessel="reactor"
      temp="24.0"
      time="60 mins"
      stir="True" />
    <Add
      reagent="saturated aqueous NH4Cl"
      vessel="reactor"
      volume="150 mL"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="all" />
    <Add
      reagent="CH2Cl2"
      vessel="reactor"
      volume="10 mL"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="all" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="buffer_flask1"
      product_bottom="True"
      solvent=""
      waste_phase_to_vessel="separator" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="True"
      solvent="CH2Cl2"
      solvent_volume="150 mL"
      n_separations="4" />
    <Transfer
      from_vessel="buffer_flask1"
      to_vessel="separator"
      volume="all" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="True"
      solvent="saturated aqueous NH4Cl"
      solvent_volume="200 mL" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="True"
      solvent="saturated aqueous NaHCO3"
      solvent_volume="200 mL" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="True"
      solvent="saturated aqueous NaCl"
      through="Na2SO4"
      solvent_volume="200 mL" />
    <Add
      reagent="dichloromethane"
      vessel="rotavap"
      volume="25 mL"
      through="Na2SO4" />
    <Evaporate
      rotavap_name="rotavap"
      temp="40.0"
      pressure="313.30670000000003"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="CH2Cl2"
      vessel="rotavap"
      volume="0" />
    <Evaporate
      rotavap_name="rotavap"
      temp="40.0"
      pressure="313.30670000000003"
      time="30 mins"
      mode="auto" />
    <Dry
      vessel="rotavap"
      time="6 hrs"
      temp="24.0"
      vacuum_pressure="0.199983" />
  </Procedure>

</Synthesis>
