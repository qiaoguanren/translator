<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="cartridge_anhydrous sodium sulfate"
      type="cartridge"
      chemical="anhydrous sodium sulfate" />
    <Component
      id="filter"
      type="filter" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="10 % hydrochloric acid" />
    <Reagent
      id="LiCl and MeONa anhydrous THF solution" />
    <Reagent
      id="NaOH H2O solution" />
    <Reagent
      id="alkylphenones 1" />
    <Reagent
      id="anhydrous EtOH" />
    <Reagent
      id="arylhydrazine hydrochlorides 3" />
    <Reagent
      id="diethyl oxalate" />
    <Reagent
      id="methylene dichloride" />
    <Reagent
      id="trifluoroacetic acid" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="LiCl and MeONa anhydrous THF solution"
      vessel="reactor"
      volume="15 mL" />
    <HeatChill
      vessel="reactor"
      temp="65.0"
      time="3 hrs"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="0.0"
      stir="True" />
    <Add
      reagent="alkylphenones 1"
      vessel="reactor"
      volume="0"
      stir="True" />
    <Add
      reagent="diethyl oxalate"
      vessel="reactor"
      mass="0.95 g"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="25.0"
      time="3 hrs"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50.0"
      pressure="249.0"
      time="30 mins"
      mode="auto" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="reactor"
      volume="all" />
    <HeatChillToTemp
      vessel="reactor"
      temp="25.0"
      stir="True" />
    <Add
      reagent="anhydrous EtOH"
      vessel="reactor"
      volume="15 mL"
      stir="True" />
    <Add
      reagent="trifluoroacetic acid"
      vessel="reactor"
      mass="1.14 g"
      stir="True" />
    <Add
      reagent="arylhydrazine hydrochlorides 3"
      vessel="reactor"
      volume="0"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="78.5"
      time="12 hrs"
      stir="True" />
    <Add
      reagent="NaOH H2O solution"
      vessel="reactor"
      volume="2 mL"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="78.5"
      time="4 hrs"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50.0"
      pressure="129.33333333333334"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="water"
      vessel="rotavap"
      volume="15 mL" />
    <Add
      reagent="methylene dichloride"
      vessel="rotavap"
      volume="15 mL"
      stir="True" />
    <Add
      reagent="10 % hydrochloric acid"
      vessel="rotavap"
      volume="10 mL"
      stir="True" />
    <Separate
      purpose="extract"
      from_vessel="rotavap"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="True"
      solvent="methylene dichloride"
      solvent_volume="10 mL"
      n_separations="2" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="True"
      solvent="water"
      through="anhydrous sodium sulfate"
      solvent_volume="20 mL"
      n_separations="2" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50.0"
      pressure="145.5"
      time="30 mins"
      mode="auto" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="filter"
      volume="all" />
    <Recrystallize
      vessel="filter" />
  </Procedure>

</Synthesis>
