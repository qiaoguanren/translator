<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="buffer_flask1"
      type="flask" />
    <Component
      id="cartridge_MgSO4"
      type="cartridge"
      chemical="MgSO4" />
    <Component
      id="cartridge_celite"
      type="cartridge"
      chemical="celite" />
    <Component
      id="filter"
      type="filter" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="1 M HCl solution" />
    <Reagent
      id="2-phenylpyridine" />
    <Reagent
      id="DCM" />
    <Reagent
      id="DI water" />
    <Reagent
      id="dichloromethane" />
    <Reagent
      id="distilled hexanes" />
    <Reagent
      id="iridium(III)chloride anhydrous" />
  </Reagents>

  <Procedure>
    <Add
      reagent="iridium(III)chloride anhydrous"
      vessel="reactor"
      mass="0.65 g" />
    <Add
      reagent="2-phenylpyridine"
      vessel="reactor"
      volume="3.74 mL" />
    <Add
      reagent="DI water"
      vessel="reactor"
      volume="650 mL"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="205.0"
      time="48 hrs"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="20.0"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="all" />
    <Add
      reagent="DCM"
      vessel="separator"
      volume="25 mL"
      stir="True" />
    <Add
      reagent="DCM"
      vessel="separator"
      volume="2.5 l"
      stir="True" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="buffer_flask1"
      product_bottom="True"
      solvent=""
      waste_phase_to_vessel="separator" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="True"
      solvent="DCM"
      solvent_volume="10 mL"
      n_separations="3" />
    <Transfer
      from_vessel="buffer_flask1"
      to_vessel="separator"
      volume="all" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="filter"
      product_bottom="True"
      solvent="1 M HCl solution"
      through="celite"
      solvent_volume="900 mL"
      n_separations="3"
      waste_phase_to_vessel="separator" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="filter"
      product_bottom="True"
      solvent="DCM"
      through="celite"
      solvent_volume="10 mL"
      n_separations="3" />
    <FilterThrough
      from_vessel="filter"
      to_vessel="filter"
      through="MgSO4" />
    <Filter
      filter_vessel="filter" />
    <Transfer
      from_vessel="filter"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="35.0"
      pressure="39.9966"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="distilled hexanes"
      vessel="rotavap"
      volume="600 mL" />
    <Sonicate />
    <Add
      reagent="dichloromethane"
      vessel="rotavap"
      volume="5 mL"
      stir="True" />
    <Stir
      vessel="rotavap"
      time="5 mins" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="filter"
      volume="all" />
    <Filter
      filter_vessel="filter" />
    <Dry
      vessel="filter" />
  </Procedure>

</Synthesis>
