<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="cartridge_celite"
      type="cartridge"
      chemical="celite" />
    <Component
      id="filter"
      type="filter" />
    <Component
      id="rotavap"
      type="rotavap" />
  </Hardware>

  <Reagents>
    <Reagent
      id="2,6-diisopropylaniline and HOAc MeOH solution" />
    <Reagent
      id="2M 1,1,1,2,2,2 hexachloroethane solution" />
    <Reagent
      id="CH2Cl2" />
    <Reagent
      id="EtOAc" />
    <Reagent
      id="KOtBu THF solution" />
    <Reagent
      id="MeOH" />
    <Reagent
      id="N,N'-1,3-bis(2,6-diisopropylphenyl)imidazolium chloride(S2)" />
    <Reagent
      id="N,N'-1,4-bis(2,6-diisopropylphenyl)-1,4-diaza-butadiene(S1)" />
    <Reagent
      id="THF" />
    <Reagent
      id="TMSCl EtOAc solution" />
    <Reagent
      id="glyoxal MeOH solution" />
    <Reagent
      id="paraformaldehyde" />
    <Reagent
      id="toluene" />
  </Reagents>

  <Procedure>
    <Add
      reagent="2,6-diisopropylaniline and HOAc MeOH solution"
      vessel="filter"
      volume="25.2 mL" />
    <HeatChillToTemp
      vessel="filter"
      temp="50.0"
      stir="True" />
    <Add
      reagent="glyoxal MeOH solution"
      vessel="filter"
      volume="18 mL"
      stir="True" />
    <Stir
      vessel="filter"
      time="15 mins" />
    <HeatChill
      vessel="filter"
      temp="23.0"
      time="10 hrs"
      stir="True" />
    <Filter
      filter_vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="MeOH"
      volume="15 mL"
      repeat="3" />
    <Dry
      vessel="filter"
      vacuum_pressure="75.0" />
    <Add
      reagent="paraformaldehyde"
      vessel="filter"
      mass="0.75 g" />
    <Add
      reagent="N,N'-1,4-bis(2,6-diisopropylphenyl)-1,4-diaza-butadiene(S1)"
      vessel="filter"
      mass="8.9 g" />
    <Add
      reagent="EtOAc"
      vessel="filter"
      volume="50 mL" />
    <HeatChillToTemp
      vessel="filter"
      temp="70.0"
      stir="True" />
    <Add
      reagent="TMSCl EtOAc solution"
      vessel="filter"
      volume="18.3 mL"
      time="45 mins"
      stir="True"
      stir_speed="600.0" />
    <Stir
      vessel="filter"
      time="2 hrs" />
    <HeatChillToTemp
      vessel="filter"
      temp="10.0"
      stir="True" />
    <Filter
      filter_vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="EtOAc"
      volume="25 mL"
      repeat="3" />
    <Dry
      vessel="filter"
      time="2 hrs"
      vacuum_pressure="75.0" />
    <HeatChillToTemp
      vessel="filter"
      temp="23.0"
      stir="True" />
    <Add
      reagent="N,N'-1,3-bis(2,6-diisopropylphenyl)imidazolium chloride(S2)"
      vessel="filter"
      mass="8.7 g" />
    <Add
      reagent="THF"
      vessel="filter"
      volume="10 mL" />
    <Add
      reagent="KOtBu THF solution"
      vessel="filter"
      volume="26 mL"
      stir="True" />
    <Stir
      vessel="filter"
      time="4 hrs" />
    <HeatChillToTemp
      vessel="filter"
      temp="-35.0"
      stir="True" />
    <Add
      reagent="2M 1,1,1,2,2,2 hexachloroethane solution"
      vessel="filter"
      volume="13 mL"
      stir="True" />
    <HeatChillToTemp
      vessel="filter"
      temp="23.0"
      stir="True" />
    <Stir
      vessel="filter"
      time="24 hrs" />
    <HeatChillToTemp
      vessel="filter"
      temp="-35.0"
      stir="True" />
    <Filter
      filter_vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="THF"
      volume="10 mL"
      temp="-20.0"
      repeat="3" />
    <WashSolid
      vessel="filter"
      solvent="toluene"
      volume="10 mL"
      temp="23.0"
      repeat="6" />
    <Dry
      vessel="filter" />
    <Dissolve
      vessel="filter"
      solvent="CH2Cl2"
      volume="50 mL" />
    <FilterThrough
      from_vessel="filter"
      to_vessel="rotavap"
      through="celite"
      eluting_solvent="CH2Cl2"
      eluting_volume="15 mL"
      eluting_repeats="3" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50.0"
      pressure="699.0"
      time="30 mins"
      mode="auto" />
  </Procedure>

</Synthesis>
