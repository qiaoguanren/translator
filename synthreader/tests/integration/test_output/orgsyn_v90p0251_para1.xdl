<?xdl version="0.2.0" ?>

<Synthesis>
  <Hardware>
    <Component
      id="buffer_flask"
      type="buffer_flask" />
    <Component
      id="cartridge_sodium sulfate"
      type="cartridge"
      chemical="sodium sulfate" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="2-aminophenethyl alcohol DME solution" />
    <Reagent
      id="MTBE" />
    <Reagent
      id="SOCl2" />
    <Reagent
      id="anhydrous DME" />
    <Reagent
      id="brine" />
    <Reagent
      id="ethyl acetate" />
    <Reagent
      id="sodium hydroxide" />
    <Reagent
      id="tert-Butyl methyl ether" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <HeatChillToTemp
      vessel="reactor"
      temp="25°C" />
    <Add
      reagent="anhydrous DME"
      vessel="reactor"
      volume="80 mL"
      stir="False" />
    <Add
      reagent="SOCl2"
      vessel="reactor"
      volume="6.2 mL"
      stir="True" />
    <Add
      reagent="2-aminophenethyl alcohol DME solution"
      vessel="reactor"
      volume="20 mL"
      time="1.25 hrs"
      stir="True" />
    <Stir
      vessel="reactor"
      time="6.5 hrs" />
    <HeatChillToTemp
      vessel="reactor"
      temp="32°C" />
    <Add
      reagent="sodium hydroxide"
      vessel="reactor"
      volume="128 mL"
      time="30 mins"
      stir="True" />
    <Add
      reagent="water"
      vessel="reactor"
      volume="16 mL"
      time="30 mins"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="60°C" />
    <Stir
      vessel="reactor"
      time="10 hrs" />
    <HeatChillToTemp
      vessel="reactor"
      temp="25°C" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="all" />
    <Add
      reagent="tert-Butyl methyl ether"
      vessel="separator"
      volume="100 mL"
      stir="True" />
    <Add
      reagent="water"
      vessel="separator"
      volume="56 mL"
      stir="True" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="buffer_flask"
      product_bottom="False"
      solvent=""
      n_separations="1"
      waste_phase_to_vessel="separator" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="MTBE"
      solvent_volume="56 mL"
      n_separations="1" />
    <Transfer
      from_vessel="buffer_flask"
      to_vessel="separator"
      volume="all" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent="brine"
      through="sodium sulfate"
      solvent_volume="43 mL"
      n_separations="1" />
    <Evaporate
      rotavap_name="rotavap"
      temp="35°C"
      pressure="79.9932 mbar"
      time="30 mins"
      mode="auto" />
    <Dissolve
      vessel="rotavap"
      solvent="ethyl acetate"
      volume="90 mL" />
  </Procedure>

</Synthesis>
