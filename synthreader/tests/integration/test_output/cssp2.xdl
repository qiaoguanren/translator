<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="filter"
      type="filter" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="distilled tris(2-aminoethyl)amine(TREN) dry tetrahydrofuran solution" />
    <Reagent
      id="lithium butyl (2.50 M in hexanes)" />
    <Reagent
      id="pentane" />
    <Reagent
      id="sublimed tert-butylchlorodimethylsilane tetrahydrofuran solution" />
    <Reagent
      id="tetrahydrofuran" />
  </Reagents>

  <Procedure>
    <HeatChillToTemp
      vessel="reactor"
      temp="-50.0"
      stir="True" />
    <Add
      reagent="distilled tris(2-aminoethyl)amine(TREN) dry tetrahydrofuran solution"
      vessel="reactor"
      volume="200 mL" />
    <Add
      reagent="lithium butyl (2.50 M in hexanes)"
      vessel="reactor"
      volume="98.5 mL"
      dispense_speed="3.0"
      stir="True" />
    <StopHeatChill
      vessel="reactor" />
    <Stir
      vessel="reactor"
      time="30 mins" />
    <HeatChillToTemp
      vessel="reactor"
      temp="0.0"
      active="False"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="-50.0"
      stir="True" />
    <Add
      reagent="sublimed tert-butylchlorodimethylsilane tetrahydrofuran solution"
      vessel="reactor"
      volume="40 mL"
      time="5 mins"
      stir="True" />
    <StartHeatChill
      vessel="reactor"
      temp="0.0" />
    <Stir
      vessel="reactor"
      time="30 mins"
      stir_speed="50.0" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50.0"
      pressure="249.0"
      time="30 mins"
      mode="auto" />
    <Separate
      purpose="extract"
      from_vessel="rotavap"
      separation_vessel="separator"
      to_vessel="filter"
      product_bottom="False"
      solvent="pentane"
      solvent_volume="50 mL"
      n_separations="3" />
    <HeatChillToTemp
      vessel="filter"
      temp="-80.0"
      stir="True" />
    <Add
      reagent="lithium butyl (2.50 M in hexanes)"
      vessel="filter"
      volume="98 mL"
      dispense_speed="3.0"
      stir="True" />
    <HeatChillToTemp
      vessel="filter"
      temp="-30.0"
      active="False"
      stir="True" />
    <Stir
      vessel="filter"
      time="15 mins" />
    <HeatChillToTemp
      vessel="filter"
      temp="-50.0"
      stir="True" />
    <Add
      reagent="tetrahydrofuran"
      vessel="filter"
      volume="18 mL"
      stir="True" />
    <Filter
      filter_vessel="filter" />
    <Dry
      vessel="filter" />
  </Procedure>

</Synthesis>
