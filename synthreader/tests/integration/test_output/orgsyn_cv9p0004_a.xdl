<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="cartridge_sodium sulfate"
      type="cartridge"
      chemical="sodium sulfate" />
    <Component
      id="column"
      type="column" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="1-Carbethoxy-4-piperidone" />
    <Reagent
      id="2-bromopropene" />
    <Reagent
      id="3-(S)- [(tert-butyldiphenylsilyl)oxy ] -2-butanone2 dry tetrahydrofuran solution" />
    <Reagent
      id="brine" />
    <Reagent
      id="dry tetrahydrofuran" />
    <Reagent
      id="ethyl acetate" />
    <Reagent
      id="p-toluenesulfonic acid" />
    <Reagent
      id="saturated aqueous ammonium chloride(NH4Cl)" />
    <Reagent
      id="tert-butyllithium 1.9 M pentane solution solution" />
    <Reagent
      id="tetrabutylammonium fluoride 1.0 M tetrahydrofuran solution solution" />
  </Reagents>

  <Procedure>
    <Add
      reagent="dry tetrahydrofuran"
      vessel="reactor"
      volume="100 mL" />
    <Add
      reagent="2-bromopropene"
      vessel="reactor"
      volume="7.7 mL"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="-70.0"
      stir="True" />
    <Add
      reagent="tert-butyllithium 1.9 M pentane solution solution"
      vessel="reactor"
      volume="92 mL"
      time="20 mins"
      stir="True" />
    <Stir
      vessel="reactor"
      time="10 mins" />
    <Add
      reagent="3-(S)- [(tert-butyldiphenylsilyl)oxy ] -2-butanone2 dry tetrahydrofuran solution"
      vessel="reactor"
      volume="50 mL"
      time="20 mins"
      stir="True" />
    <Stir
      vessel="reactor"
      time="30 mins" />
    <Add
      reagent="tetrabutylammonium fluoride 1.0 M tetrahydrofuran solution solution"
      vessel="reactor"
      volume="163 mL"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="23.0"
      stir="True" />
    <Stir
      vessel="reactor"
      time="60 mins" />
    <Add
      reagent="saturated aqueous ammonium chloride(NH4Cl)"
      vessel="rotavap"
      volume="200 mL" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50.0"
      pressure="541.5"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="brine"
      vessel="rotavap"
      volume="200 mL" />
    <Separate
      purpose="extract"
      from_vessel="rotavap"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="True"
      solvent="ethyl acetate"
      solvent_volume="200 mL"
      n_separations="2" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent="brine"
      through="sodium sulfate"
      solvent_volume="100 mL"
      n_separations="5" />
    <Evaporate
      rotavap_name="rotavap"
      time="30 mins"
      mode="auto" />
    <Distill />
    <Transfer
      from_vessel="rotavap"
      to_vessel="reactor"
      volume="all" />
    <Add
      reagent="1-Carbethoxy-4-piperidone"
      vessel="reactor"
      mass="7.52 g" />
    <Add
      reagent="p-toluenesulfonic acid"
      vessel="reactor"
      mass="5 g" />
    <HeatChill
      vessel="reactor"
      temp="100.0"
      time="1.5 hrs"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="23.0"
      stir="True" />
    <RunColumn
      from_vessel="reactor"
      to_vessel="reactor"
      column="column" />
  </Procedure>

</Synthesis>
