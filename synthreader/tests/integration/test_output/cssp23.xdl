<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor" />
  </Hardware>

  <Reagents>
    <Reagent
      id="DMSO" />
    <Reagent
      id="absolute ethanol" />
    <Reagent
      id="dry ether" />
    <Reagent
      id="hydrazine hydrate" />
    <Reagent
      id="palladium chloride" />
    <Reagent
      id="triphenylphosphine" />
  </Reagents>

  <Procedure>
    <Add
      reagent="palladium chloride"
      vessel="reactor"
      volume="0" />
    <Add
      reagent="DMSO"
      vessel="reactor"
      volume="0" />
    <Add
      reagent="triphenylphosphine"
      vessel="reactor"
      volume="0" />
    <HeatChillToTemp
      vessel="reactor"
      temp="140.0"
      stir="True" />
    <StopHeatChill
      vessel="reactor" />
    <Stir
      vessel="reactor"
      time="15 mins" />
    <Add
      reagent="hydrazine hydrate"
      vessel="reactor"
      volume="0" />
    <HeatChillToTemp
      vessel="reactor"
      temp="125.0"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="25.0"
      active="False"
      continue_heatchill="False"
      stir="True" />
    <WashSolid
      vessel="reactor"
      solvent="absolute ethanol"
      volume="10 mL"
      repeat="2" />
    <WashSolid
      vessel="reactor"
      solvent="dry ether"
      volume="10 mL"
      repeat="2" />
    <Dry
      vessel="reactor"
      time="3.5 hrs" />
  </Procedure>

</Synthesis>
