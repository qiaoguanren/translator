<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="filter"
      type="filter" />
    <Component
      id="rotavap"
      type="rotavap" />
  </Hardware>

  <Reagents>
    <Reagent
      id="dichloromethane" />
    <Reagent
      id="dodecylamine" />
    <Reagent
      id="dry pyridine" />
    <Reagent
      id="oxone" />
    <Reagent
      id="pyridinium bromide" />
    <Reagent
      id="the 4-acetamidoTEMPO catalyst" />
  </Reagents>

  <Procedure>
    <Dry
      vessel="filter" />
    <HeatChillToTemp
      vessel="filter"
      temp="25.0"
      active="False"
      continue_heatchill="False"
      stir="True" />
    <Add
      reagent="dichloromethane"
      vessel="filter"
      volume="200 mL" />
    <Add
      reagent="oxone"
      vessel="filter"
      mass="80 g"
      stir="True" />
    <Add
      reagent="pyridinium bromide"
      vessel="filter"
      mass="0.365 g"
      stir="True" />
    <Stir
      vessel="filter"
      time="15 mins" />
    <Add
      reagent="dry pyridine"
      vessel="filter"
      volume="20 mL"
      dispense_speed="10.0"
      stir="True" />
    <Stir
      vessel="filter"
      time="1.5 mins" />
    <Add
      reagent="the 4-acetamidoTEMPO catalyst"
      vessel="filter"
      mass="0.533 g"
      stir="True" />
    <Stir
      vessel="filter"
      time="22.5 mins" />
    <Add
      reagent="dodecylamine"
      vessel="filter"
      mass="9.24 g"
      stir="True" />
    <Add
      reagent="dichloromethane"
      vessel="filter"
      volume="100 mL"
      stir="True" />
    <Stir
      vessel="filter"
      time="5 mins" />
    <Add
      reagent="dichloromethane"
      vessel="filter"
      volume="10 mL"
      stir="True" />
    <Add
      reagent="dichloromethane"
      vessel="filter"
      volume="20 mL"
      dispense_speed="10.0"
      stir="True" />
    <HeatChill
      vessel="filter"
      temp="23.0"
      time="12 hrs"
      stir="True" />
    <Filter
      filter_vessel="filter" />
    <Add
      reagent="dichloromethane"
      vessel="filter"
      volume="300 mL" />
    <Filter
      filter_vessel="filter" />
    <Transfer
      from_vessel="filter"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="35.0"
      pressure="666.61"
      time="30 mins"
      mode="auto" />
    <Evaporate
      rotavap_name="rotavap"
      temp="35.0"
      pressure="19.9983"
      time="22.5 mins"
      mode="auto" />
    <Repeat
      repeats="2">
      <Add
        reagent="dichloromethane"
        vessel="rotavap"
        volume="5 mL" />
    </Repeat>
    <Evaporate
      rotavap_name="rotavap"
      temp="35.0"
      pressure="666.61"
      time="30 mins"
      mode="auto" />
    <Evaporate
      rotavap_name="rotavap"
      temp="35.0"
      pressure="19.9983"
      time="22.5 mins"
      mode="auto" />
    <Distill />
  </Procedure>

</Synthesis>
