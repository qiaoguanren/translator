<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="cartridge_MgSO4"
      type="cartridge"
      chemical="MgSO4" />
    <Component
      id="column"
      type="column" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="H2O" />
    <Reagent
      id="MTBE" />
    <Reagent
      id="methyl tert-butyl ether(MTBE)" />
    <Reagent
      id="saturated aqueous NaCl" />
  </Reagents>

  <Procedure>
    <Add
      reagent="MTBE"
      vessel="reactor"
      volume="50 mL" />
    <Separate
      purpose="extract"
      from_vessel="reactor"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="H2O"
      solvent_volume="150 mL"
      n_separations="3" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent="saturated aqueous NaCl"
      through="MgSO4"
      solvent_volume="250 mL"
      n_separations="2" />
    <Add
      reagent="methyl tert-butyl ether(MTBE)"
      vessel="rotavap"
      volume="50 mL"
      through="MgSO4" />
    <Evaporate
      rotavap_name="rotavap"
      temp="40.0"
      pressure="303.30755"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="MTBE"
      vessel="rotavap"
      volume="10 mL" />
    <Evaporate
      rotavap_name="rotavap"
      temp="40.0"
      pressure="303.30755"
      time="30 mins"
      mode="auto" />
    <RunColumn
      from_vessel="rotavap"
      to_vessel="reactor"
      column="column" />
  </Procedure>

</Synthesis>
