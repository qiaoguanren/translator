<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
  </Hardware>

  <Reagents>
    <Reagent
      id="chloro(dimethyl sulfide)gold" />
    <Reagent
      id="dichloromethane" />
    <Reagent
      id="tris(2,4-di-tert-butylphenyl)phosphite" />
  </Reagents>

  <Procedure>
    <Add
      reagent="tris(2,4-di-tert-butylphenyl)phosphite"
      vessel="reactor"
      mass="0.647 g" />
    <Add
      reagent="chloro(dimethyl sulfide)gold"
      vessel="reactor"
      mass="0.295 g" />
    <Add
      reagent="dichloromethane"
      vessel="reactor"
      volume="5 mL"
      dispense_speed="10.0" />
    <HeatChill
      vessel="reactor"
      temp="23.0"
      time="60 mins"
      stir="True"
      stir_speed="800.0" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="30.0"
      pressure="399.966"
      time="30 mins"
      mode="auto" />
    <Dry
      vessel="rotavap"
      time="24 hrs"
      vacuum_pressure="1.33322" />
  </Procedure>

</Synthesis>
