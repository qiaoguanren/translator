<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="filter"
      type="filter" />
  </Hardware>

  <Reagents>
    <Reagent
      id="(S)-(-)-α,α-Diphenyl-2-pyrrolidinylmethanol" />
    <Reagent
      id="MTBE" />
    <Reagent
      id="THF" />
    <Reagent
      id="imidazole" />
    <Reagent
      id="methyl tert-butyl ether(MTBE)" />
    <Reagent
      id="tetrahydrofuran" />
    <Reagent
      id="the TMSCl" />
    <Reagent
      id="trimethylchlorosilane" />
  </Reagents>

  <Procedure>
    <Add
      reagent="(S)-(-)-α,α-Diphenyl-2-pyrrolidinylmethanol"
      vessel="filter"
      mass="25 g" />
    <Dissolve
      vessel="filter"
      solvent="THF"
      volume="220 mL" />
    <Add
      reagent="imidazole"
      vessel="filter"
      mass="20 g"
      stir="True" />
    <HeatChillToTemp
      vessel="filter"
      temp="4.0"
      stir="True" />
    <Add
      reagent="trimethylchlorosilane"
      vessel="filter"
      volume="31.3 mL"
      dispense_speed="10.0"
      time="20 mins"
      stir="True" />
    <Add
      reagent="tetrahydrofuran"
      vessel="filter"
      volume="50 mL"
      stir="True" />
    <HeatChill
      vessel="filter"
      temp="23.0"
      time="15 hrs"
      stir="True" />
    <Add
      reagent="methyl tert-butyl ether(MTBE)"
      vessel="filter"
      volume="150 mL"
      stir="True" />
    <Stir
      vessel="filter"
      time="15 mins" />
    <Filter
      filter_vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="MTBE"
      volume="50 mL"
      repeat="3" />
    <Dry
      vessel="filter" />
  </Procedure>

</Synthesis>
