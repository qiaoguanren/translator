<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="column"
      type="column" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
  </Hardware>

  <Reagents>
    <Reagent
      id="TBTH and AIBN toluene solution" />
    <Reagent
      id="unknown_reagent toluene solution" />
  </Reagents>

  <Procedure>
    <Add
      reagent="unknown_reagent toluene solution"
      vessel="reactor"
      volume="0" />
    <HeatChillToTemp
      vessel="reactor"
      temp="110.6"
      stir="True" />
    <Add
      reagent="TBTH and AIBN toluene solution"
      vessel="reactor"
      volume="2 mL" />
    <Stir
      vessel="reactor"
      time="60 mins" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50.0"
      pressure="48.0"
      time="30 mins"
      mode="auto" />
    <RunColumn
      from_vessel="rotavap"
      to_vessel="reactor"
      column="column" />
  </Procedure>

</Synthesis>
