<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="cartridge_MgSO4"
      type="cartridge"
      chemical="MgSO4" />
    <Component
      id="column"
      type="column" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="2,4,6-trimethylaniline" />
    <Reagent
      id="acetonitrile" />
    <Reagent
      id="dysprosium" />
    <Reagent
      id="ethyl acetate" />
    <Reagent
      id="furan-2-yl(phenyl)methanol" />
    <Reagent
      id="saturated aqueous sodium bicarbonate" />
  </Reagents>

  <Procedure>
    <Add
      reagent="furan-2-yl(phenyl)methanol"
      vessel="reactor"
      mass="3.95 g" />
    <Add
      reagent="acetonitrile"
      vessel="reactor"
      volume="200 mL" />
    <Add
      reagent="2,4,6-trimethylaniline"
      vessel="reactor"
      mass="2.78 g"
      stir="True" />
    <Add
      reagent="dysprosium"
      vessel="reactor"
      mass="0.628 g"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="80.0"
      stir="True" />
    <Stir
      vessel="reactor"
      time="4 hrs" />
    <StopStir
      vessel="reactor" />
    <HeatChillToTemp
      vessel="reactor"
      temp="25.0"
      active="False"
      continue_heatchill="False"
      stir="True" />
    <Add
      reagent="saturated aqueous sodium bicarbonate"
      vessel="reactor"
      volume="150 mL"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="all" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="True"
      solvent="ethyl acetate"
      through="MgSO4"
      solvent_volume="150 mL"
      n_separations="3" />
    <Evaporate
      rotavap_name="rotavap"
      pressure="13.3322"
      time="30 mins"
      mode="auto" />
    <RunColumn
      from_vessel="rotavap"
      to_vessel="reactor"
      column="column" />
  </Procedure>

</Synthesis>
