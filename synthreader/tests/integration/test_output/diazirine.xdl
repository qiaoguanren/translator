<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="filter"
      type="filter" />
    <Component
      id="rotavap"
      type="rotavap" />
  </Hardware>

  <Reagents>
    <Reagent
      id="anhydrous MgSO4" />
    <Reagent
      id="methanolic ammonia solution" />
    <Reagent
      id="methanolic levulinic acid" />
    <Reagent
      id="the HOSA solution" />
  </Reagents>

  <Procedure>
    <HeatChillToTemp
      vessel="filter"
      temp="-10.0"
      stir="True" />
    <Add
      reagent="anhydrous MgSO4"
      vessel="filter"
      mass="2 g" />
    <Add
      reagent="methanolic ammonia solution"
      vessel="filter"
      volume="80 mL" />
    <Add
      reagent="methanolic levulinic acid"
      vessel="filter"
      volume="9.3 mL"
      stir="True" />
    <Stir
      vessel="filter"
      time="3 hrs" />
    <Add
      reagent="the HOSA solution"
      vessel="filter"
      volume="60 mL"
      dispense_speed="3.0"
      stir="True" />
    <HeatChillToTemp
      vessel="filter"
      temp="20.0"
      stir="True" />
    <Stir
      vessel="filter"
      time="15 hrs" />
    <Filter
      filter_vessel="filter" />
    <Transfer
      from_vessel="filter"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      time="30 mins"
      mode="auto" />
  </Procedure>

</Synthesis>
