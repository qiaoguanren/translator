<?xdl version="0.2.0" ?>

<Synthesis>
  <Hardware>
    <Component
      id="cartridge_celite"
      type="cartridge"
      chemical="celite" />
    <Component
      id="filter"
      type="filter" />
    <Component
      id="rotavap"
      type="rotavap" />
  </Hardware>

  <Reagents>
    <Reagent
      id="1M 1,1,1,2,2,2 hexachloroethane solution" />
    <Reagent
      id="CH2Cl2" />
    <Reagent
      id="KOtBu" />
    <Reagent
      id="N,N'-1,3-bis(2,6-diisopropylphenyl)imidazolium chloride(S2) THF solution" />
    <Reagent
      id="THF" />
    <Reagent
      id="toluene" />
  </Reagents>

  <Procedure>
    <HeatChillToTemp
      vessel="filter"
      temp="23°C" />
    <Add
      reagent="N,N'-1,3-bis(2,6-diisopropylphenyl)imidazolium chloride(S2) THF solution"
      vessel="filter"
      volume="700 mL"
      stir="False" />
    <Add
      reagent="KOtBu"
      vessel="filter"
      mass="47.4 g"
      stir="True" />
    <Stir
      vessel="filter"
      time="4 hrs" />
    <HeatChillToTemp
      vessel="filter"
      temp="-40°C" />
    <Add
      reagent="1M 1,1,1,2,2,2 hexachloroethane solution"
      vessel="filter"
      volume="423 mL"
      stir="True" />
    <HeatChillToTemp
      vessel="filter"
      temp="23°C" />
    <Stir
      vessel="filter"
      time="24 hrs" />
    <HeatChillToTemp
      vessel="filter"
      temp="-40°C" />
    <Filter
      filter_vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="THF"
      volume="100 mL"
      temp="-40°C"
      repeat="3" />
    <WashSolid
      vessel="filter"
      solvent="toluene"
      volume="100 mL"
      temp="-40°C"
      repeat="6" />
    <Dry
      vessel="filter" />
    <Dissolve
      vessel="filter"
      solvent="CH2Cl2"
      volume="500 mL" />
    <FilterThrough
      from_vessel="filter"
      to_vessel="rotavap"
      through="celite"
      eluting_solvent="CH2Cl2"
      eluting_volume="50 mL"
      eluting_repeats="3" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50°C"
      pressure="699 mbar"
      time="30 mins"
      mode="auto" />
  </Procedure>

</Synthesis>
