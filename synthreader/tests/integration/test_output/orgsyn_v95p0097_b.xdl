<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="buffer_flask1"
      type="flask" />
    <Component
      id="cartridge_Na2SO4"
      type="cartridge"
      chemical="Na2SO4" />
    <Component
      id="cartridge_cotton wool"
      type="cartridge"
      chemical="cotton wool" />
    <Component
      id="column"
      type="column" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="6H-benzo [ c ] chromen-6-one 2" />
    <Reagent
      id="acetonitrile" />
    <Reagent
      id="deionized water" />
    <Reagent
      id="dichloromethane" />
    <Reagent
      id="iodomethane" />
    <Reagent
      id="potassium hydroxide(KOH)pellets" />
    <Reagent
      id="saturated NaCl solution" />
  </Reagents>

  <Procedure>
    <Add
      reagent="acetonitrile"
      vessel="reactor"
      volume="60 mL" />
    <Add
      reagent="6H-benzo [ c ] chromen-6-one 2"
      vessel="reactor"
      mass="5.89 g"
      stir="True" />
    <Add
      reagent="iodomethane"
      vessel="reactor"
      mass="42.6 g"
      stir="True" />
    <Stir
      vessel="reactor"
      time="5 mins" />
    <Add
      reagent="potassium hydroxide(KOH)pellets"
      vessel="reactor"
      mass="9.9 g"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="23.0"
      time="24 hrs"
      stir="True"
      stir_speed="600.0" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50.0"
      pressure="153.0"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="dichloromethane"
      vessel="rotavap"
      volume="50 mL" />
    <Add
      reagent="deionized water"
      vessel="rotavap"
      volume="50 mL"
      stir="True" />
    <Separate
      purpose="extract"
      from_vessel="rotavap"
      separation_vessel="separator"
      to_vessel="buffer_flask1"
      product_bottom="True"
      solvent=""
      waste_phase_to_vessel="separator" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="True"
      solvent="dichloromethane"
      solvent_volume="50 mL"
      n_separations="3" />
    <Transfer
      from_vessel="buffer_flask1"
      to_vessel="separator"
      volume="all" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="True"
      solvent="saturated NaCl solution"
      through="Na2SO4"
      solvent_volume="100 mL"
      waste_phase_to_vessel="separator" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="True"
      solvent="dichloromethane"
      through="Na2SO4"
      solvent_volume="50 mL" />
    <FilterThrough
      from_vessel="rotavap"
      to_vessel="rotavap"
      through="cotton wool" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50.0"
      pressure="370.5"
      time="30 mins"
      mode="auto" />
    <RunColumn
      from_vessel="rotavap"
      to_vessel="reactor"
      column="column" />
  </Procedure>

</Synthesis>
