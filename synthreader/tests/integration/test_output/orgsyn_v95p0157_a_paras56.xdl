<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="buffer_flask1"
      type="flask" />
    <Component
      id="cartridge_Na2SO4"
      type="cartridge"
      chemical="Na2SO4" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="CH2Cl2" />
    <Reagent
      id="Et2O" />
    <Reagent
      id="THF" />
    <Reagent
      id="compound 3" />
    <Reagent
      id="diethyl ether" />
    <Reagent
      id="saturated aqueous NaCl" />
    <Reagent
      id="saturated aqueous NaHCO3" />
    <Reagent
      id="tetrabutylammonium fluoride" />
    <Reagent
      id="tetrahydrofuran" />
  </Reagents>

  <Procedure>
    <Evaporate
      rotavap_name="rotavap"
      temp="40.0"
      pressure="313.30670000000003"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="compound 3"
      vessel="reactor"
      mass="17.7 g" />
    <Add
      reagent="THF"
      vessel="reactor"
      volume="750 mL" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="reactor"
      volume="all" />
    <Add
      reagent="THF"
      vessel="rotavap"
      volume="15 mL"
      stir="True" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="reactor"
      volume="15 mL" />
    <HeatChillToTemp
      vessel="reactor"
      temp="4.0"
      stir="True" />
    <Add
      reagent="tetrabutylammonium fluoride"
      vessel="reactor"
      volume="58 mL"
      time="1.5 hrs"
      stir="True" />
    <Add
      reagent="tetrahydrofuran"
      vessel="reactor"
      volume="10 mL"
      stir="True" />
    <Stir
      vessel="reactor"
      time="60 mins" />
    <Add
      reagent="saturated aqueous NaHCO3"
      vessel="reactor"
      volume="120 mL"
      stir="True" />
    <Stir
      vessel="reactor"
      time="10 mins" />
    <Add
      reagent="Et2O"
      vessel="reactor"
      volume="300 mL"
      stir="True" />
    <Separate
      purpose="extract"
      from_vessel="reactor"
      separation_vessel="separator"
      to_vessel="buffer_flask1"
      product_bottom="False"
      solvent=""
      waste_phase_to_vessel="separator" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="Et2O"
      solvent_volume="150 mL"
      n_separations="3" />
    <Transfer
      from_vessel="buffer_flask1"
      to_vessel="separator"
      volume="all" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="saturated aqueous NaHCO3"
      solvent_volume="150 mL" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent="saturated aqueous NaCl"
      through="Na2SO4"
      solvent_volume="150 mL" />
    <Add
      reagent="diethyl ether"
      vessel="rotavap"
      volume="200 mL"
      through="Na2SO4" />
    <Evaporate
      rotavap_name="rotavap"
      temp="40.0"
      pressure="353.30330000000004"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="CH2Cl2"
      vessel="rotavap"
      volume="0" />
    <Evaporate
      rotavap_name="rotavap"
      temp="40.0"
      pressure="286.64230000000003"
      time="30 mins"
      mode="auto" />
    <Dry
      vessel="rotavap"
      time="10 hrs"
      temp="24.0"
      vacuum_pressure="0.199983" />
    <RunColumn
      from_vessel="rotavap"
      to_vessel="reactor" />
  </Procedure>

</Synthesis>
