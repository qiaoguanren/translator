<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="filter"
      type="filter" />
  </Hardware>

  <Reagents>
    <Reagent
      id="anhydrous diethyl ether" />
    <Reagent
      id="deionized water" />
    <Reagent
      id="ethanol" />
    <Reagent
      id="glacial acetic acid" />
    <Reagent
      id="trans-4 methoxy-3-buten-2-one" />
  </Reagents>

  <Procedure>
    <Add
      reagent="trans-4 methoxy-3-buten-2-one"
      vessel="filter"
      volume="15.3 mL" />
    <Add
      reagent="ethanol"
      vessel="filter"
      volume="120 mL"
      stir="True" />
    <Add
      reagent="deionized water"
      vessel="filter"
      volume="30 mL"
      stir="True" />
    <Add
      reagent="glacial acetic acid"
      vessel="filter"
      volume="0.9 mL"
      dispense_speed="10.0"
      stir="True" />
    <HeatChill
      vessel="filter"
      temp="77.0"
      time="48 hrs"
      stir="True" />
    <HeatChillToTemp
      vessel="filter"
      temp="25.0"
      active="False"
      continue_heatchill="False"
      stir="True" />
    <HeatChill
      vessel="filter"
      temp="-20.0"
      time="18 hrs"
      stir="True" />
    <Filter
      filter_vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="anhydrous diethyl ether"
      volume="50 mL"
      temp="0.0"
      repeat="4" />
    <Dry
      vessel="filter" />
    <Dissolve
      vessel="filter"
      solvent="ethanol"
      volume="200 mL"
      temp="78.0" />
    <HeatChillToTemp
      vessel="filter"
      temp="25.0"
      active="False"
      continue_heatchill="False"
      stir="True" />
    <HeatChill
      vessel="filter"
      temp="-20.0"
      time="18 hrs"
      stir="True" />
    <Filter
      filter_vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="anhydrous diethyl ether"
      volume="50 mL"
      temp="0.0"
      repeat="4" />
    <Dry
      vessel="filter"
      time="16 hrs" />
  </Procedure>

</Synthesis>
