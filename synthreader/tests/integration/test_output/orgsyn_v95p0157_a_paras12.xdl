<?xdl version="0.4.4" ?>

<Synthesis>
  <Hardware>
    <Component
      id="cartridge_Na2SO4"
      type="cartridge"
      chemical="Na2SO4" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="CH2Cl2" />
    <Reagent
      id="EtOAc" />
    <Reagent
      id="H2O" />
    <Reagent
      id="MeOH" />
    <Reagent
      id="a mixture of ice-water ( 170 mL )" />
    <Reagent
      id="ethyl acetate" />
    <Reagent
      id="saturated aqueous NH4Cl" />
    <Reagent
      id="saturated aqueous NaCl" />
    <Reagent
      id="sodium borohydride" />
    <Reagent
      id="the aldehyde 1" />
  </Reagents>

  <Procedure>
    <Confirm
      msg="Is The aldehyde 1 in the correct vessel?" />
    <Dissolve
      vessel="reactor"
      solvent="MeOH"
      volume="200 mL" />
    <HeatChillToTemp
      vessel="reactor"
      temp="-20.0"
      stir="True" />
    <Repeat
      repeats="10">
      <Add
        reagent="sodium borohydride"
        vessel="reactor"
        mass="0.3 g"
        stir="True" />
      <Wait
        time="3 mins" />
    </Repeat>
    <Stir
      vessel="reactor"
      time="45 mins" />
    <HeatChillToTemp
      vessel="reactor"
      temp="0.0"
      active="False"
      stir="True" />
    <Stir
      vessel="reactor"
      time="35 mins" />
    <Add
      reagent="a mixture of ice-water ( 170 mL )"
      vessel="reactor"
      volume="170 mL"
      stir="True"
      stir_speed="600.0" />
    <Stir
      vessel="reactor"
      time="10 mins" />
    <Add
      reagent="EtOAc"
      vessel="reactor"
      volume="900 mL"
      stir="True" />
    <Add
      reagent="H2O"
      vessel="reactor"
      volume="50 mL"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="all" />
    <Add
      reagent="ethyl acetate"
      vessel="reactor"
      volume="50 mL"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="50 mL" />
    <Add
      reagent="H2O"
      vessel="reactor"
      volume="50 mL"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="50 mL" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="EtOAc"
      solvent_volume="150 mL"
      n_separations="3" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="saturated aqueous NH4Cl"
      solvent_volume="100 mL" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent="saturated aqueous NaCl"
      through="Na2SO4"
      solvent_volume="100 mL" />
    <Add
      reagent="EtOAc"
      vessel="rotavap"
      volume="50 mL"
      through="Na2SO4" />
    <Evaporate
      rotavap_name="rotavap"
      temp="40.0"
      pressure="113.3237"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="CH2Cl2"
      vessel="rotavap"
      volume="0" />
    <Evaporate
      rotavap_name="rotavap"
      temp="40.0"
      pressure="313.30670000000003"
      time="30 mins"
      mode="auto" />
    <Dry
      vessel="rotavap"
      time="5 hrs"
      temp="24.0"
      vacuum_pressure="0.199983" />
  </Procedure>

</Synthesis>
