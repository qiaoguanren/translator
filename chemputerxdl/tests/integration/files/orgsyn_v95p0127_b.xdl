<?xdl version="0.4.2" ?>

<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor" />
  </Hardware>

  <Reagents>
    <Reagent
      id="3-Bromopyridine" />
    <Reagent
      id="dry THF" />
    <Reagent
      id="iPrMgCl·LiCl" />
    <Reagent
      id="solid Zn(OPiv)2" />
  </Reagents>

  <Procedure>
    <Add
      reagent="3-Bromopyridine"
      vessel="reactor"
      mass="6.32 g" />
    <Add
      reagent="dry THF"
      vessel="reactor"
      volume="50 mL"
      dispense_speed="10" />
    <HeatChillToTemp
      vessel="reactor"
      temp="0°C"
      stir="True" />
    <Stir
      vessel="reactor"
      time="5 mins" />
    <Add
      reagent="iPrMgCl·LiCl"
      vessel="reactor"
      volume="35.2 mL"
      time="30 mins"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="25°C"
      time="3 hrs"
      stir="True" />
    <Add
      reagent="solid Zn(OPiv)2"
      vessel="reactor"
      mass="12.3 g"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="25°C"
      time="30 mins"
      stir="True" />
    <Dry
      vessel="reactor"
      vacuum_pressure="0.1333 mbar" />
    <Dry
      vessel="reactor"
      time="2 hrs" />
    <Dry
      vessel="reactor"
      vacuum_pressure="0.1333 mbar" />
  </Procedure>

</Synthesis>
