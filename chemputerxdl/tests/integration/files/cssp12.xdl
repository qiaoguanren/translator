<?xdl version="0.4.2" ?>

<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="Bu3SnCl" />
    <Reagent
      id="ether" />
    <Reagent
      id="t-BuLi" />
    <Reagent
      id="the indole THF solution" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <HeatChillToTemp
      vessel="reactor"
      temp="-78째C"
      stir="True" />
    <Add
      reagent="the indole THF solution"
      vessel="reactor"
      volume="0" />
    <Add
      reagent="t-BuLi"
      vessel="reactor"
      volume="0"
      dispense_speed="3" />
    <Wait
      time="5 mins" />
    <Add
      reagent="Bu3SnCl"
      vessel="reactor"
      volume="0" />
    <Stir
      vessel="reactor"
      time="20 mins" />
    <HeatChillToTemp
      vessel="reactor"
      temp="25째C"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50째C"
      pressure="249 mbar"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="water"
      vessel="rotavap"
      volume="0" />
    <Separate
      purpose="extract"
      from_vessel="rotavap"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent="ether"
      solvent_volume="25 mL" />
    <Dry
      vessel="rotavap" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50째C"
      pressure="376.3333 mbar"
      time="30 mins"
      mode="auto" />
    <RunColumn
      from_vessel="rotavap"
      to_vessel="reactor"
      column="column" />
  </Procedure>

</Synthesis>
