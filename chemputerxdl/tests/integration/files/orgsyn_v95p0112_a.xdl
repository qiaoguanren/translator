<?xdl version="0.4.2" ?>

<Synthesis>
  <Hardware>
    <Component
      id="cartridge_alumina"
      type="cartridge"
      chemical="alumina" />
    <Component
      id="filter"
      type="filter" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
  </Hardware>

  <Reagents>
    <Reagent
      id="1-Amino pyridinium iodide" />
    <Reagent
      id="acetone" />
    <Reagent
      id="dichloromethane" />
    <Reagent
      id="dichloromethane-methanol" />
    <Reagent
      id="diethyl ether" />
    <Reagent
      id="methanol" />
    <Reagent
      id="methyl(tert-butoxycarbonyl)-glycinate 1" />
    <Reagent
      id="potassium carbonate" />
  </Reagents>

  <Procedure>
    <Add
      reagent="methyl(tert-butoxycarbonyl)-glycinate 1"
      vessel="reactor"
      mass="6.8 g" />
    <Add
      reagent="methanol"
      vessel="reactor"
      volume="225 mL" />
    <Add
      reagent="1-Amino pyridinium iodide"
      vessel="reactor"
      mass="6.67 g"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="22°C"
      time="5 mins"
      stir="True" />
    <Add
      reagent="potassium carbonate"
      vessel="reactor"
      mass="9.95 g"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="22°C"
      time="64 hrs"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="40°C"
      pressure="179.9847 mbar"
      time="30 mins"
      mode="auto" />
    <FilterThrough
      from_vessel="rotavap"
      to_vessel="rotavap"
      through="alumina" />
    <Add
      reagent="dichloromethane"
      vessel="rotavap"
      volume="10 mL"
      stir="True" />
    <Add
      reagent="dichloromethane-methanol"
      vessel="rotavap"
      volume="50 mL"
      stir="True" />
    <FilterThrough
      from_vessel="rotavap"
      to_vessel="rotavap"
      through="alumina"
      eluting_solvent="dichloromethane-methanol"
      eluting_volume="1.1 l" />
    <Evaporate
      rotavap_name="rotavap"
      temp="40°C"
      pressure="299.9745 mbar"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="dichloromethane"
      vessel="rotavap"
      volume="20 mL" />
    <Evaporate
      rotavap_name="rotavap"
      temp="40°C"
      pressure="259.9779 mbar"
      time="30 mins"
      mode="auto" />
    <Dry
      vessel="rotavap"
      temp="20°C"
      vacuum_pressure="0.1067 mbar" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="filter"
      volume="all" />
    <Add
      reagent="acetone"
      vessel="filter"
      volume="175 mL"
      stir="True" />
    <HeatChill
      vessel="filter"
      temp="56.05°C"
      time="15 mins"
      stir="True" />
    <HeatChillToTemp
      vessel="filter"
      temp="25°C"
      active="False"
      continue_heatchill="False"
      stir="True" />
    <HeatChill
      vessel="filter"
      temp="-22°C"
      time="20 hrs"
      stir="True" />
    <Filter
      filter_vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="diethyl ether"
      volume="25 mL"
      repeat="3" />
    <Dry
      vessel="filter" />
  </Procedure>

</Synthesis>
