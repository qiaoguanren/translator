<?xdl version="0.4.2" ?>

<Synthesis>
  <Hardware>
    <Component
      id="cartridge_anhydrous sodium sulfate"
      type="cartridge"
      chemical="anhydrous sodium sulfate" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="10 % hydrochloric acid" />
    <Reagent
      id="LiCl and EtONa anhydrous THF solution" />
    <Reagent
      id="diethyl oxalate" />
    <Reagent
      id="methylene chloride" />
    <Reagent
      id="propiophenone 1a" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="LiCl and EtONa anhydrous THF solution"
      vessel="reactor"
      volume="15 mL" />
    <HeatChill
      vessel="reactor"
      temp="65°C"
      time="3 hrs"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="0°C"
      stir="True" />
    <Add
      reagent="propiophenone 1a"
      vessel="reactor"
      mass="0.67 g"
      stir="True" />
    <Add
      reagent="diethyl oxalate"
      vessel="reactor"
      mass="0.95 g"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="25°C"
      time="3 hrs"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50°C"
      pressure="249 mbar"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="water"
      vessel="rotavap"
      volume="15 mL" />
    <Add
      reagent="methylene chloride"
      vessel="rotavap"
      volume="15 mL"
      stir="True" />
    <Add
      reagent="10 % hydrochloric acid"
      vessel="rotavap"
      volume="3 mL"
      stir="True" />
    <Separate
      purpose="extract"
      from_vessel="rotavap"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="True"
      solvent="methylene chloride"
      solvent_volume="10 mL"
      n_separations="2" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="True"
      solvent="water"
      through="anhydrous sodium sulfate"
      solvent_volume="20 mL"
      n_separations="2" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50°C"
      pressure="145.5 mbar"
      time="30 mins"
      mode="auto" />
    <RunColumn
      from_vessel="rotavap"
      to_vessel="reactor"
      column="column" />
  </Procedure>

</Synthesis>
