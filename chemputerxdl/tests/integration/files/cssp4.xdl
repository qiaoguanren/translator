<?xdl version="0.4.2" ?>

<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
  </Hardware>

  <Reagents>
    <Reagent
      id="DEAD" />
    <Reagent
      id="THF" />
    <Reagent
      id="the alcohol" />
    <Reagent
      id="the maleimide and triphenylphosphine THF solution" />
  </Reagents>

  <Procedure>
    <HeatChillToTemp
      vessel="reactor"
      temp="0°C"
      stir="True" />
    <Add
      reagent="the maleimide and triphenylphosphine THF solution"
      vessel="reactor"
      volume="0" />
    <Add
      reagent="the alcohol"
      vessel="reactor"
      volume="0"
      dispense_speed="3" />
    <Add
      reagent="THF"
      vessel="reactor"
      volume="10 mL"
      dispense_speed="3" />
    <Add
      reagent="DEAD"
      vessel="reactor"
      volume="0"
      dispense_speed="3"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="18°C"
      active="False"
      continue_heatchill="False"
      stir="True" />
    <Stir
      vessel="reactor"
      time="2.5 hrs" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50°C"
      pressure="249 mbar"
      time="30 mins"
      mode="auto" />
    <RunColumn
      from_vessel="rotavap"
      to_vessel="reactor"
      column="column" />
  </Procedure>

</Synthesis>
