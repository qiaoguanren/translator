<?xdl version="0.4.2" ?>

<Synthesis>
  <Hardware>
    <Component
      id="cartridge_magnesium sulphate"
      type="cartridge"
      chemical="magnesium sulphate" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="ammonium chloride solution" />
    <Reagent
      id="brine" />
    <Reagent
      id="ether" />
    <Reagent
      id="pyrrolidine" />
    <Reagent
      id="the 2 alkynes" />
    <Reagent
      id="the copper iodide" />
  </Reagents>

  <Procedure>
    <HeatChillToTemp
      vessel="reactor"
      temp="0Â°C"
      stir="True" />
    <Add
      reagent="the 2 alkynes"
      vessel="reactor"
      volume="0"
      stir="True" />
    <Add
      reagent="pyrrolidine"
      vessel="reactor"
      volume="0"
      stir="True" />
    <Stir
      vessel="reactor"
      time="5 mins" />
    <Add
      reagent="the copper iodide"
      vessel="reactor"
      volume="0" />
    <Stir
      vessel="reactor"
      time="45 mins" />
    <Add
      reagent="ammonium chloride solution"
      vessel="reactor"
      volume="0" />
    <Separate
      purpose="extract"
      from_vessel="reactor"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="ether"
      solvent_volume="25 mL"
      n_separations="4" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent="brine"
      through="magnesium sulphate"
      solvent_volume="25 mL" />
    <Evaporate
      rotavap_name="rotavap"
      time="30 mins"
      mode="auto" />
    <RunColumn
      from_vessel="rotavap"
      to_vessel="reactor"
      column="column" />
  </Procedure>

</Synthesis>
