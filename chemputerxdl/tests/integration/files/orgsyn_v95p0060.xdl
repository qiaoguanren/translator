      reagent="dry pyridine"
      vessel="filter"
      volume="20 mL"
      dispense_speed="10"
      stir="True" />
    <Stir
      vessel="filter"
      time="1.5 mins" />
    <Add
      reagent="the 4-acetamidoTEMPO catalyst"
      vessel="filter"
      mass="0.533 g"
      stir="True" />
    <Stir
      vessel="filter"
      time="22.5 mins" />
    <Add
      reagent="dodecylamine"
      vessel="filter"
      mass="9.24 g"
      stir="True" />
    <Add
      reagent="dichloromethane"
      vessel="filter"
      volume="100 mL"
      stir="True" />
    <Stir
      vessel="filter"
      time="5 mins" />
    <Add
      reagent="dichloromethane"
      vessel="filter"
      volume="10 mL"
      stir="True" />
    <Add
      reagent="dichloromethane"
      vessel="filter"
      volume="20 mL"
      dispense_speed="10"
      stir="True" />
