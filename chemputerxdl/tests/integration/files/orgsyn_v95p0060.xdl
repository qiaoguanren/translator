      id="rotavap"
      type="rotavap" />
  </Hardware>

  <Reagents>
    <Reagent
      id="dichloromethane" />
    <Reagent
      id="dodecylamine" />
    <Reagent
      id="dry pyridine" />
    <Reagent
      id="oxone" />
    <Reagent
      id="pyridinium bromide" />
    <Reagent
      id="the 4-acetamidoTEMPO catalyst" />
  </Reagents>

  <Procedure>
    <Dry
      vessel="filter" />
    <HeatChillToTemp
      vessel="filter"
      temp="25°C"
      active="False"
      continue_heatchill="False"
      stir="True" />
    <Add
      reagent="dichloromethane"
      vessel="filter"
      volume="200 mL" />
    <Add
      reagent="oxone"
      vessel="filter"
      mass="80 g"
      stir="True" />
    <Add
      reagent="pyridinium bromide"
      vessel="filter"
      mass="0.365 g"
      stir="True" />
    <Stir
      vessel="filter"
      time="15 mins" />
