      vessel="filter"
      temp="23°C"
      time="12 hrs"
      stir="True" />
    <Filter
      filter_vessel="filter" />
    <Add
      reagent="dichloromethane"
      vessel="filter"
      volume="300 mL" />
    <Filter
      filter_vessel="filter" />
    <Transfer
      from_vessel="filter"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="35°C"
      pressure="666.61 mbar"
      time="30 mins"
      mode="auto" />
    <Evaporate
      rotavap_name="rotavap"
      temp="35°C"
      pressure="19.9983 mbar"
      time="22.5 mins"
      mode="auto" />
    <Add
      reagent="dichloromethane"
      vessel="rotavap"
      volume="5 mL"
      repeat="2" />
    <Evaporate
      rotavap_name="rotavap"
      temp="35°C"
      pressure="666.61 mbar"
      time="30 mins"
      mode="auto" />
    <Evaporate
      rotavap_name="rotavap"
      temp="35°C"
      pressure="19.9983 mbar"
      time="22.5 mins"
      mode="auto" />
