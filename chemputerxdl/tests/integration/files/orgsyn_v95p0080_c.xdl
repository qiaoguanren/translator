<?xdl version="0.4.2" ?>

<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="(S)-DTBM-SEGPHOS" />
    <Reagent
      id="2,3,3-trimethyl-1-butene" />
    <Reagent
      id="N,N-dibenzyl-O-pivaloylhydroxylamine" />
    <Reagent
      id="THF" />
    <Reagent
      id="copper(II)acetate" />
    <Reagent
      id="dimethoxy(methyl)silane" />
    <Reagent
      id="ethyl acetate" />
    <Reagent
      id="saturated aqueous sodium carbonate" />
    <Reagent
      id="triphenylphosphine" />
  </Reagents>

  <Procedure>
    <Add
      reagent="N,N-dibenzyl-O-pivaloylhydroxylamine"
      vessel="reactor"
      mass="9.09 g" />
    <Add
      reagent="copper(II)acetate"
      vessel="reactor"
      mass="46 mg" />
    <Add
      reagent="(S)-DTBM-SEGPHOS"
      vessel="reactor"
      mass="0.33 g" />
    <Add
      reagent="triphenylphosphine"
      vessel="reactor"
      mass="74 mg" />
    <Add
      reagent="2,3,3-trimethyl-1-butene"
      vessel="reactor"
      volume="3.55 mL" />
    <Add
      reagent="THF"
      vessel="reactor"
      volume="25 mL"
      dispense_speed="10"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="40°C"
      stir="True" />
    <Add
      reagent="dimethoxy(methyl)silane"
      vessel="reactor"
      volume="6.27 mL"
      time="10 mins"
      stir="True" />
    <Stir
      vessel="reactor"
      time="12 hrs" />
    <HeatChillToTemp
      vessel="reactor"
      temp="23°C"
      stir="True" />
    <Add
      reagent="saturated aqueous sodium carbonate"
      vessel="reactor"
      volume="50 mL"
      dispense_speed="10"
      stir="True" />
    <Add
      reagent="ethyl acetate"
      vessel="reactor"
      volume="50 mL"
      stir="True" />
    <Separate
      purpose="extract"
      from_vessel="reactor"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent=""
      waste_phase_to_vessel="separator" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent="ethyl acetate"
      solvent_volume="50 mL"
      n_separations="2" />
    <Evaporate
      rotavap_name="rotavap"
      temp="35°C"
      pressure="66.661 mbar"
      time="30 mins"
      mode="auto" />
    <RunColumn
      from_vessel="rotavap"
      to_vessel="reactor"
      column="column" />
  </Procedure>

</Synthesis>
