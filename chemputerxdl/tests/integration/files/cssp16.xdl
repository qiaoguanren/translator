<?xdl version="0.4.2" ?>

<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="BuLi" />
    <Reagent
      id="THF" />
    <Reagent
      id="TsF THF solution" />
    <Reagent
      id="ether" />
    <Reagent
      id="ice-brine solution" />
    <Reagent
      id="indole THF solution" />
    <Reagent
      id="t-BuLi" />
  </Reagents>

  <Procedure>
    <HeatChillToTemp
      vessel="reactor"
      temp="-78°C"
      stir="True" />
    <Add
      reagent="indole THF solution"
      vessel="reactor"
      volume="40 mL" />
    <Add
      reagent="BuLi"
      vessel="reactor"
      volume="0"
      dispense_speed="3"
      stir="True" />
    <StopHeatChill
      vessel="reactor" />
    <Wait
      time="10 mins" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50°C"
      pressure="249 mbar"
      time="30 mins"
      mode="auto" />
    <Dissolve
      vessel="rotavap"
      solvent="THF"
      volume="40 mL" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="reactor"
      volume="all" />
    <HeatChillToTemp
      vessel="reactor"
      temp="-78°C"
      stir="True" />
    <Add
      reagent="t-BuLi"
      vessel="reactor"
      volume="0"
      dispense_speed="3"
      stir="True" />
    <Stir
      vessel="reactor"
      time="20 mins" />
    <Add
      reagent="TsF THF solution"
      vessel="reactor"
      volume="9.91 mL"
      stir="True" />
    <Stir
      vessel="reactor"
      time="60 mins" />
    <HeatChillToTemp
      vessel="reactor"
      temp="18°C"
      active="False"
      continue_heatchill="False"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="25°C"
      time="2 hrs"
      stir="True" />
    <Add
      reagent="ice-brine solution"
      vessel="reactor"
      volume="0"
      stir="True" />
    <Separate
      purpose="extract"
      from_vessel="reactor"
      separation_vessel="separator"
      to_vessel="reactor"
      product_bottom="False"
      solvent="ether"
      solvent_volume="25 mL" />
    <RunColumn
      from_vessel="reactor"
      to_vessel="reactor"
      column="column" />
  </Procedure>

</Synthesis>
