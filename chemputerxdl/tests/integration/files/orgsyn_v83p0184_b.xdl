      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="True"
      solvent="dichloromethane"
      solvent_volume="50 mL"
      n_separations="2" />
    <Transfer
      from_vessel="buffer_flask"
      to_vessel="separator"
      volume="all" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="filter"
      product_bottom="True"
      solvent="copper sulfate(CuSO4) water solution"
      through="anhydrous magnesium sulfate"
      solvent_volume="100 mL"
      n_separations="2" />
