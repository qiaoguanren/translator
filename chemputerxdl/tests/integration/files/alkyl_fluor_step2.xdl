<?xdl version="0.2.0" ?>

<Synthesis>
  <Hardware>
    <Component
      id="filter"
      type="filter" />
  </Hardware>

  <Reagents>
    <Reagent
      id="EtOAc" />
    <Reagent
      id="N,N'-1,4-bis(2,6-diisopropylphenyl)-1,4-diaza-butadiene(S1) and paraformaldehyde EtOAc solution" />
    <Reagent
      id="TMSCl EtOAc solution" />
  </Reagents>

  <Procedure>
    <Add
      reagent="N,N'-1,4-bis(2,6-diisopropylphenyl)-1,4-diaza-butadiene(S1) and paraformaldehyde EtOAc solution"
      vessel="filter"
      volume="5.4 l"
      time="45 mins"
      stir="True"
      stir_speed="600 RPM" />
    <HeatChillToTemp
      vessel="filter"
      temp="70°C" />
    <Add
      reagent="TMSCl EtOAc solution"
      vessel="filter"
      volume="156.5 mL"
      time="45 mins"
      stir="True"
      stir_speed="600 RPM" />
    <Stir
      vessel="filter"
      time="2 hrs" />
    <HeatChillToTemp
      vessel="filter"
      temp="10°C" />
    <Filter
      filter_vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="EtOAc"
      volume="500 mL"
      repeat="3" />
    <Dry
      vessel="filter" />
  </Procedure>

</Synthesis>
