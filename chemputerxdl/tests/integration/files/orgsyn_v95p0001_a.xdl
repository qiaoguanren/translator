<?xdl version="0.4.2" ?>

<Synthesis>
  <Hardware>
    <Component
      id="buffer_flask"
      type="buffer_flask" />
    <Component
      id="cartridge_anhydrous MgSO4"
      type="cartridge"
      chemical="anhydrous MgSO4" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="1 M HCl" />
    <Reagent
      id="N,N-dimethylaminopyridine" />
    <Reagent
      id="acetic anhydride" />
    <Reagent
      id="acetophenone" />
    <Reagent
      id="brine" />
    <Reagent
      id="ethanol" />
    <Reagent
      id="ethyl acetate" />
    <Reagent
      id="hydroxylamine hydrochloride" />
    <Reagent
      id="pyridine" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="acetophenone"
      vessel="reactor"
      volume="9.33 mL" />
    <Add
      reagent="pyridine"
      vessel="reactor"
      volume="18 mL"
      stir="True" />
    <Add
      reagent="ethanol"
      vessel="reactor"
      volume="40 mL"
      stir="True" />
    <Add
      reagent="hydroxylamine hydrochloride"
      vessel="reactor"
      mass="8.33 g"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="60°C"
      time="1.25 hrs"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="22°C"
      stir="True" />
    <Add
      reagent="water"
      vessel="reactor"
      volume="80 mL"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="all" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="buffer_flask"
      product_bottom="False"
      solvent=""
      waste_phase_to_vessel="separator" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="ethyl acetate"
      solvent_volume="80 mL"
      n_separations="2" />
    <Transfer
      from_vessel="buffer_flask"
      to_vessel="separator"
      volume="all" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="1 M HCl"
      solvent_volume="50 mL" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent="brine"
      through="anhydrous MgSO4"
      solvent_volume="50 mL" />
    <Evaporate
      rotavap_name="rotavap"
      temp="35°C"
      pressure="39.9966 mbar"
      time="30 mins"
      mode="auto" />
    <Dry
      vessel="rotavap"
      temp="22°C"
      vacuum_pressure="1.9998 mbar" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="reactor"
      volume="all" />
    <Add
      reagent="N,N-dimethylaminopyridine"
      vessel="reactor"
      mass="20 mg" />
    <Add
      reagent="pyridine"
      vessel="reactor"
      volume="35 mL" />
    <Add
      reagent="acetic anhydride"
      vessel="reactor"
      volume="15 mL"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="24°C"
      time="60 mins"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all" />
    <Evaporate
      rotavap_name="rotavap"
      temp="45°C"
      pressure="46.6627 mbar"
      time="30 mins"
      mode="auto" />
    <Add
      reagent="water"
      vessel="rotavap"
      volume="50 mL" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="separator"
      volume="all" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="buffer_flask"
      product_bottom="False"
      solvent=""
      waste_phase_to_vessel="separator" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="ethyl acetate"
      solvent_volume="50 mL"
      n_separations="2" />
    <Transfer
      from_vessel="buffer_flask"
      to_vessel="separator"
      volume="all" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="1 M HCl"
      solvent_volume="50 mL" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent="brine"
      through="anhydrous MgSO4"
      solvent_volume="50 mL" />
    <Evaporate
      rotavap_name="rotavap"
      temp="35°C"
      pressure="39.9966 mbar"
      time="30 mins"
      mode="auto" />
  </Procedure>

</Synthesis>
