<?xdl version="0.4.2" ?>

<Synthesis>
  <Hardware>
    <Component
      id="cartridge_magnesium sulphate"
      type="cartridge"
      chemical="magnesium sulphate" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
  </Hardware>

  <Reagents>
    <Reagent
      id="ammonium chloride" />
    <Reagent
      id="brine" />
    <Reagent
      id="diethylamine" />
    <Reagent
      id="ether" />
    <Reagent
      id="the alkyne" />
    <Reagent
      id="the copper iodide" />
    <Reagent
      id="the palladium tetrakis(triphenylphosphine)" />
    <Reagent
      id="vinyl halide" />
  </Reagents>

  <Procedure>
    <HeatChillToTemp
      vessel="reactor"
      temp="25°C"
      stir="True" />
    <Add
      reagent="the palladium tetrakis(triphenylphosphine)"
      vessel="reactor"
      volume="0"
      stir="True" />
    <Add
      reagent="diethylamine"
      vessel="reactor"
      volume="100 mL"
      stir="True" />
    <Add
      reagent="vinyl halide"
      vessel="reactor"
      volume="0"
      stir="True" />
    <Stir
      vessel="reactor"
      time="15 mins" />
    <StopHeatChill
      vessel="reactor" />
    <Add
      reagent="the copper iodide"
      vessel="reactor"
      volume="0"
      stir="True" />
    <Add
      reagent="the alkyne"
      vessel="reactor"
      volume="0"
      stir="True" />
    <HeatChill
      vessel="reactor"
      temp="55.5°C"
      time="4 hrs"
      stir="True" />
    <Add
      reagent="ammonium chloride"
      vessel="reactor"
      volume="0"
      stir="True" />
    <Separate
      purpose="extract"
      from_vessel="reactor"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="ether"
      solvent_volume="25 mL" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent="brine"
      through="magnesium sulphate"
      solvent_volume="25 mL" />
    <Evaporate
      rotavap_name="rotavap"
      time="30 mins"
      mode="auto" />
    <RunColumn
      from_vessel="rotavap"
      to_vessel="reactor"
      column="column" />
  </Procedure>

</Synthesis>
