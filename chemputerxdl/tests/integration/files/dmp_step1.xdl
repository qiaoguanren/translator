<?xdl version="0.2.0" ?>

<Synthesis>
  <Hardware>
    <Component
      id="filter"
      type="filter" />
  </Hardware>

  <Reagents>
    <Reagent
      id="2-Iodobenzoic acid" />
    <Reagent
      id="acetone" />
    <Reagent
      id="oxone deionized water solution" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="oxone deionized water solution"
      vessel="filter"
      volume="650 mL"
      stir="True"
      stir_speed="600 RPM" />
    <Add
      reagent="2-Iodobenzoic acid"
      vessel="filter"
      mass="50 g"
      stir="True"
      stir_speed="600 RPM" />
    <HeatChillToTemp
      vessel="filter"
      temp="71.5°C" />
    <Stir
      vessel="filter"
      time="3 hrs" />
    <HeatChillToTemp
      vessel="filter"
      temp="5°C" />
    <Stir
      vessel="filter"
      time="1.5 hrs"
      stir_speed="50 RPM" />
    <Filter
      filter_vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="water"
      volume="100 mL"
      anticlogging="False"
      repeat="6" />
    <WashSolid
      vessel="filter"
      solvent="acetone"
      volume="100 mL"
      anticlogging="False"
      repeat="2" />
    <Dry
      vessel="filter"
      time="2 hrs"
      temp="25°C" />
  </Procedure>

</Synthesis>
