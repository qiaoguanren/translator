<Synthesis>
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="ether" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="water"
      vessel="rotavap"
      volume="300 mL" />
    <FilterThrough
      from_vessel="rotavap"
      to_vessel="rotavap"
      through="celite"
      eluting_solvent="ether"
      eluting_volume="25 mL"
      eluting_repeats="3" />
  </Procedure>

</Synthesis>
