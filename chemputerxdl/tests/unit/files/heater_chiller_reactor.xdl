<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor"
    />
  </Hardware>

  <Reagents />

  <Procedure>
    <StartHeatChill
      temp="150"
      vessel="reactor" />
    <StopHeatChill
      vessel="reactor" />

    <HeatChillToTemp
      vessel="reactor"
      temp="150" />
    <HeatChill
      vessel="reactor"
      temp="150"
      time="30"
    />

    <StartHeatChill
      temp="50"
      vessel="reactor" />
    <StopHeatChill
      vessel="reactor" />

    <HeatChillToTemp
      vessel="reactor"
      temp="50" />
    <HeatChill
      vessel="reactor"
      temp="50"
      time="30"
    />

    <StartHeatChill
      temp="0"
      vessel="reactor" />
    <StopHeatChill
      vessel="reactor" />

    <HeatChillToTemp
      vessel="reactor"
      temp="0" />
    <HeatChill
      vessel="reactor"
      temp="0"
      time="30"
    />
  </Procedure>

</Synthesis>
