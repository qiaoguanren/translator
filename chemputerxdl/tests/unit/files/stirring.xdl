<Synthesis>
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
    <Component
      id="filter"
      type="filter"
    />
    <Component
      id="reactor"
      type="reactor"
    />
  </Hardware>

  <Reagents />

  <Procedure>
    <StartStir
      vessel="filter" />
    <StopStir
      vessel="filter" />
    <StartStir
      vessel="reactor" />
    <StopStir
      vessel="reactor" />
    <Stir
      vessel="rotavap"
      time="1 min"
      continue_stirring="True"/>
    <StopStir
      vessel="rotavap" />
  </Procedure>

</Synthesis>
