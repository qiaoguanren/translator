<Synthesis>
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="NaCl" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="NaCl"
      vessel="rotavap"
      mass="1g" />
    <WashSolid
      vessel="rotavap"
      solvent="water"
      volume="100mL" />
  </Procedure>

</Synthesis>
