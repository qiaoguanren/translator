<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor"
     />
    <Component
      id="rotavap"
      type="rotavap"
     />
  </Hardware>

  <Reagents>
    <Reagent
      id="NaCl" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="water"
      vessel="reactor"
      volume="300 mL" />
    <Filter
      filter_vessel="reactor"
      filtrate_vessel="rotavap"
    />

  </Procedure>

</Synthesis>
