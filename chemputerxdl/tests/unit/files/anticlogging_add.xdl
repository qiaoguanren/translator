<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor"
    />
  </Hardware>

  <Reagents>
    <Reagent id="ether" />
    <Reagent id="water" />
  </Reagents>

  <Procedure>
    <Add
      vessel="reactor"
      reagent="ether"
      volume="60 mL"
      anticlogging="true"
      anticlogging_solvent="water" />
    <Add
      vessel="reactor"
      reagent="ether"
      volume="60 mL"
      anticlogging="true"
      anticlogging_solvent="water"
      anticlogging_solvent_volume="3 mL"
      anticlogging_reagent_volume="20 mL"
    />
  </Procedure>

</Synthesis>
