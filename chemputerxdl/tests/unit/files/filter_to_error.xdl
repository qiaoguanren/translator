<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor"
    />
    <Component
      id="rotavap"
      type="rotavap"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="NaCl" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="water"
      vessel="rotavap"
      volume="300 mL" />
    <FilterTo
      from_vessel="rotavap"
      to_vessel="reactor"
    />

  </Procedure>

</Synthesis>
