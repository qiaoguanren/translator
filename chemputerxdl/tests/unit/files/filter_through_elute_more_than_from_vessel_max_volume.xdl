<Synthesis>
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
    <Component
      id="reactor"
      type="reactor"
    />
    <Component
      id="product"
      type="flask"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="ether" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <FilterThrough
      from_vessel="reactor"
      to_vessel="product"
      through="celite"
      eluting_solvent="ether"
      eluting_volume="1 L"
      eluting_repeats="1" />
    <FilterThrough
      from_vessel="reactor"
      to_vessel="product"
      through="celite"
      eluting_solvent="ether"
      eluting_volume="25 mL"
      eluting_repeats="3" />
    <FilterThrough
      from_vessel="reactor"
      to_vessel="product"
      through="celite"
      eluting_solvent="ether"
      eluting_volume="50 mL"
      eluting_repeats="3" />
  </Procedure>

</Synthesis>
