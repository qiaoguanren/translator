<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor"
    />
    <Component
      id="filter"
      type="filter"
    />
  </Hardware>

  <Reagents />

  <Procedure>
    <StartPurge
      vessel="reactor"
    />
    <StopPurge
      vessel="reactor"
    />
    <Purge
      vessel="reactor"
      time="5 mins"
    />

    <StartPurge
      vessel="filter"
    />
    <StopPurge
      vessel="filter"
    />
    <Purge
      vessel="filter"
      time="5 mins"
    />
  </Procedure>

</Synthesis>
