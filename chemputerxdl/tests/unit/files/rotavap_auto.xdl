<Synthesis>
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="water"
      vessel="rotavap"
      volume="500 mL" />
    <Evaporate
      rotavap_name="rotavap"
      mode="auto"
      temp="40"
      time="30mins" />

  </Procedure>

</Synthesis>
