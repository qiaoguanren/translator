<Synthesis>
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
    <Component
      id="reactor"
      type="reactor"
    />
    <Component
      id="separator"
      type="separator"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="ether" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="water"
      vessel="reactor"
      volume="300 mL" />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="10 mL"
      through="celite" />
    <FilterThrough
      from_vessel="reactor"
      to_vessel="rotavap"
      through="celite" />
    <Separate
      purpose="wash"
      from_vessel="reactor"
      separation_vessel="separator"
      to_vessel="rotavap"
      to_port="evaporate"
      solvent="ether"
      product_bottom="False"
      solvent_volume="50 mL"
      n_separations="1"
      through="celite" />
  </Procedure>

</Synthesis>
