<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor"
    />
    <Component
      id="rotavap"
      type="rotavap"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="water"
      vessel="reactor"
      volume="100 mL"
    />
    <StartStir
      vessel="reactor"
    />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="10 mL"
    />
    <Transfer
      from_vessel="reactor"
      to_vessel="rotavap"
      volume="all"
    />
  </Procedure>

</Synthesis>
