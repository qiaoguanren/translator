<Synthesis filter_dead_volume_method="solvent">
  <Hardware>
    <Component
      id="reactor"
      type="reactor"
    />
  </Hardware>

  <Reagents>
    <Reagent id="NaCl" />
    <Reagent id="ether" />
  </Reagents>

  <Procedure>
    <Add
      vessel="reactor"
      reagent="NaCl"
      mass="5g"
    />
    <Dry
      vessel="reactor"
      vacuum_pressure="600"
      time="2 hrs"
    />

  </Procedure>

</Synthesis>
