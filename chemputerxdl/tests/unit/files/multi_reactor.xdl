<Synthesis>
  <Hardware>
    <Component
      id="reactor0"
      type="reactor"
    />
    <Component
      id="reactor1"
      type="reactor"
    />
    <Component
      id="reactor2"
      type="reactor"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="water"
      vessel="reactor0"
      volume="30 mL" />
    <Transfer
      from_vessel="reactor0"
      to_vessel="reactor1"
      volume="all"/>
    <Add
      reagent="water"
      vessel="reactor1"
      volume="30 mL" />
    <Add
      reagent="water"
      vessel="reactor2"
      volume="30 mL" />
  </Procedure>
</Synthesis>
