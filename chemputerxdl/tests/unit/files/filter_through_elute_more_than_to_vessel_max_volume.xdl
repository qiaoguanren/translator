<Synthesis>
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
    <Component
      id="product"
      type="flask"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="ether" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>

    <FilterThrough
      from_vessel="reactor"
      to_vessel="product"
      through="celite"
      eluting_solvent="ether"
      eluting_volume="1 L"
      eluting_repeats="2" />

  </Procedure>

</Synthesis>
