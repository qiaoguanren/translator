<Synthesis>
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
    <Component
      id="filter"
      type="filter"
    />
    <Component
      id="reactor"
      type="reactor"
    />
  </Hardware>

  <Reagents />

  <Procedure>
    <StartHeatChill
      temp="50"
      vessel="filter" />
    <StopHeatChill
      vessel="filter" />
    <StartHeatChill
      temp="60"
      vessel="reactor" />
    <StopHeatChill
      vessel="reactor" />
    <StartHeatChill
      temp="60"
      vessel="rotavap" />
    <StopHeatChill
      vessel="rotavap" />

    <HeatChillToTemp
      vessel="reactor"
      temp="50" />
    <HeatChillToTemp
      vessel="filter"
      temp="50" />
    <HeatChillToTemp
      vessel="rotavap"
      temp="50" />
  </Procedure>

</Synthesis>
