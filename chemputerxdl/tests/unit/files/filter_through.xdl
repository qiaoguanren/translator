<Synthesis>
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
    <Component
      id="reactor"
      type="reactor"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="NaCl" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="water"
      vessel="reactor"
      volume="300 mL" />
    <FilterThrough
      from_vessel="reactor"
      to_vessel="rotavap"
      through="celite"
    />
      <!-- eluting_solvent="ether"
      eluting_volume="50 mL"
      eluting_repeats="3" /> -->

  </Procedure>

</Synthesis>
