<Synthesis>
  <Hardware>
    <Component
      id="reactor"
      type="reactor"
    />
    <Component
      id="filter"
      type="filter"
    />
  </Hardware>

  <Reagents>
    <Reagent id="NaCl" />
  </Reagents>

  <Procedure>
    <Add
      vessel="reactor"
      reagent="NaCl"
      mass="5g"
      port="0"
    />
    <Dry
      vessel="filter"
      vacuum_pressure="600"
      time="2 hrs"
    />

  </Procedure>

</Synthesis>
