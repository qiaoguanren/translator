<?xdl version="0.4.0" ?>

<Synthesis>
  <Hardware>
    <Component
      id="cartridge_anhydrous magnesium sulfate(MgSO4)"
      type="cartridge"
      chemical="anhydrous magnesium sulfate(MgSO4)" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
    <Component
      id="waste_X"
      type="flask"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="1,2-dibromoethane" />
    <Reagent
      id="50 % aqueous NaOH solution" />
    <Reagent
      id="brine" />
    <Reagent
      id="concentrated hydrochloric acid" />
    <Reagent
      id="diethyl malonate" />
    <Reagent
      id="ether" />
    <Reagent
      id="toluene" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="1,2-dibromoethane"
      vessel="reactor"
      volume="3.3 mL"
      stir="False" />
    <Add
      reagent="diethyl malonate"
      vessel="reactor"
      volume="3.83 mL"
      stir="True" />
    <Add
      reagent="50 % aqueous NaOH solution"
      vessel="reactor"
      volume="50 mL"
      stir="True"
      stir_speed="250 RPM" />
    <HeatChill
      vessel="reactor"
      temp="25°C"
      time="2 hrs"
      stir_speed="250 RPM" />
    <HeatChillToTemp
      vessel="reactor"
      temp="15°C" />
    <Add
      reagent="concentrated hydrochloric acid"
      vessel="reactor"
      volume="65 mL"
      dispense_speed="1"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="25°C" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="500 mL" />
    <Add
      reagent="water"
      vessel="reactor"
      volume="10 mL"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="15 mL" />
    <Add
      reagent="water"
      vessel="reactor"
      volume="10 mL"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="15 mL" />
    <Add
      reagent="water"
      vessel="reactor"
      volume="10 mL"
      stir="True" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="15 mL" />
    <StopHeatChill
      vessel="reactor" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="ether"
      solvent_volume="30 mL"
      n_separations="6" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent="brine"
      through="anhydrous magnesium sulfate(MgSO4)"
      solvent_volume="50 mL"
      n_separations="1" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50°C"
      pressure="440 mbar"
      time="30 mins"
      mode="auto" />
    <Dry
      vessel="rotavap"
      time="10 mins"
      temp="50°C" />
    <Add
      reagent="toluene"
      vessel="rotavap"
      volume="10 mL"
      stir="False" />
    <Stir
      vessel="rotavap"
      time="5 mins" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="waste_X"
      volume="25 mL" />
    <Evaporate
      rotavap_name="rotavap"
      temp="50°C"
      pressure="48 mbar"
      time="30 mins"
      mode="auto" />
    <Dry
      vessel="rotavap"
      time="30 mins"
      temp="50°C" />
  </Procedure>

</Synthesis>
