<Synthesis>
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
    <Component
      id="reactor"
      type="reactor"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="chloroacetyl chloride" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <CleanVessel
      solvent="water"
      vessel="rotavap" />
    <CleanVessel
      solvent="chloroacetyl chloride"
      vessel="reactor"
      volume="50 mL"
      stir_time="5 mins" />
  </Procedure>

</Synthesis>
