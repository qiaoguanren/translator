<Synthesis>
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
    <Component
      id="filter"
      type="filter"
    />
    <Component
      id="reactor"
      type="reactor"
    />
    <Component
      id="separator"
      type="separator"
    />
  </Hardware>

  <Reagents>
    <Reagent id="ether" />
  </Reagents>

  <Procedure>
    <Separate
      purpose="wash"
      from_vessel="filter"
      separation_vessel="separator"
      to_vessel="rotavap"
      through="mgso4"
      solvent="ether"
      product_bottom="False"
      to_port="evaporate"
      solvent_volume="50 mL"
      n_separations="1" />
    <Separate
      purpose="wash"
      from_vessel="filter"
      separation_vessel="separator"
      to_vessel="rotavap"
      through="celite"
      solvent="ether"
      product_bottom="True"
      to_port="evaporate"
      solvent_volume="50 mL"
      n_separations="1" />
    <Separate
      purpose="wash"
      from_vessel="filter"
      separation_vessel="separator"
      to_vessel="rotavap"
      through="celite"
      solvent="ether"
      product_bottom="False"
      to_port="evaporate"
      solvent_volume="50 mL"
      n_separations="2" />
    <Separate
      purpose="wash"
      from_vessel="filter"
      separation_vessel="separator"
      to_vessel="rotavap"
      through="celite"
      solvent="ether"
      product_bottom="True"
      to_port="evaporate"
      solvent_volume="50 mL"
      n_separations="2" />
  </Procedure>

</Synthesis>
