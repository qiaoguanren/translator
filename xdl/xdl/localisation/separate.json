{
  "Separate": {
    "en": {
      "purpose": {
        "purpose": {
          "wash": "Wash",
          "extract": "Extract"
        }
      },
      "mixture": {
        "solvent": {
          "any": "contents of {from_vessel}",
          "else": "two-phase mixture in {from_vessel}"
        }
      },
      "solvent": {
        "solvent": {
          "any": "with {solvent}",
          "else": "without adding solvent"
        }
      },
      "volume": {
        "solvent": {
          "any": "({n_separations} x {solvent_volume})",
          "else": ""
        }
      },
      "waste_phase": {
        "product_phase": {
          "bottom": "waste phase (top)",
          "top": "waste phase (bottom)"
        }
      },
      "waste_phase_dest": {
        "waste_phase_to_vessel": {
          "any": "to vessel",
          "else": "to waste"
        }
      },
      "product_phase_full": {
        "product_phase": {
          "bottom": "product phase (bottom)",
          "top": "product phase (top)"
        }
      },
      "through_prefix": {
        "through": {
          "any": "through",
          "else": "directly"
        }
      },
      "full": "{purpose} {mixture} {solvent} {volume}. Transfer {waste_phase} {waste_phase_dest} {waste_phase_to_vessel}, and {product_phase_full} {through_prefix} {through} to {to_vessel}"
    }
  }
}
