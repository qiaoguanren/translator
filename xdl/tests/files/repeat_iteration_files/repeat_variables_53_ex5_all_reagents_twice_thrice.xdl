<XDL>

<Synthesis>

      <Hardware>
         <Component id='reactor' type='reactor'/>
      </Hardware>

  <Reagents>
    <Reagent
      name="water"
      role="substrate" />
    <Reagent
      name="ether"
      role="substrate" />
  </Reagents>

  <Procedure>
    <Repeat repeats="3">
      <Repeat rx.kind='Reagent'>
        <Repeat repeats="2">
          <Add reagent='rx' vessel='reactor' amount='5 mL'/>
        </Repeat>
      </Repeat>
    </Repeat>
  </Procedure>

</Synthesis>
</XDL>
