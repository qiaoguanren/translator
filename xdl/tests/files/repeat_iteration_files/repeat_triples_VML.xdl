<XDL>

<Synthesis>

      <Hardware>
         <Component id='reactor' type='reactor'/>
      </Hardware>

  <Reagents>
    <Reagent
      name="water"
      role="substrate" />
    <Reagent
      name="ether"
      role="substrate" />
    <Reagent
      name="THF"
      role="substrate" />
    <Reagent
      name="chloroacetyl chloride"
      role="substrate" />
  </Reagents>

  <Procedure>
    <Repeat rx.role='substrate'>
      <Repeat>
        <Repeat repeats="2">
          <Add reagent='rx' vessel='reactor' amount='5 mL'/>
        </Repeat>
      <MockMeasure target_repeats="3"/>
      </Repeat>
    </Repeat>
  </Procedure>

</Synthesis>
</XDL>
