<XDL>
    <Blueprint id="test_BP">
        <Hardware>
            <Component id="BP_filter" type="ChemputerFilter"/>
          </Hardware>

        <Parameters>
            <Parameter id="reagent_2_vol" type="volume" value="16.2 mL"/>
        </Parameters>

        <Reagents>
            <Reagent id="reagent_1"/>
            <Reagent id="reagent_2"/>
            <Reagent id="BP_filter"/>
        </Reagents>

        <Procedure>
            <Add
                vessel="BP_filter"
                reagent="reagent_1"
                amount="157.5 g"
                stir="True"
                port="top"
            />
            <Add
                vessel="BP_filter"
                reagent="reagent_2"
                param.volume="reagent_2_vol"
                stir="True"
                port="top"
            />
            <Filter
                vessel="BP_filter"
            />
            <WashSolid
                vessel="BP_filter"
                solvent="anhydrous ether"
                volume="80.0 mL"
                stir="solvent"
                repeats="3"
            />
        </Procedure>
    </Blueprint>

    <Synthesis id="DMP">

    <Hardware>
        <Component
        id="filter"
        type="ChemputerFilter"
        />
    </Hardware>

    <Reagents>
        <Reagent
        name="glacial acetic acid"
        density="1.05 g/mL"
        />
        <Reagent
        name="the moist solid iodinane oxide"
        />
        <Reagent
        name="acetone"
        />
        <Reagent
        name="Oxone deionized water solution"
        concentration="1.0 mol/L"
        />
        <Reagent
        name="2-Iodobenzoic acid"
        solid="True"
        />
        <Reagent
        name="anhydrous ether"
        />
        <Reagent
        name="acetic anhydride"
        />
        <Reagent
        name="water"
        />
    </Reagents>

    <Parameters>
        <Parameter
        id="acetic_anhydride_volume"
        type="volume"
        min="250 mL"
        max="350 mL"
        value="200 mL"
        />
        <Parameter
        id="dry_temperature_1"
        type="temperature"
        min="23째C"
        max="27째C"
        value="25.0째C"
        />
    </Parameters>

    <Procedure>
        <Add
        vessel="filter"
        reagent="Oxone deionized water solution"
        amount="650 mmol"
        port="top"
        />
        <HeatChill
        vessel="filter"
        temp="70.0 째C"
        time="1200.0 secs"
        stir="true"
        />
        <Filter
        vessel="filter"
        />
        <Dry
        vessel="filter"
        time="300.0 secs"
        param.temp="dry_temperature_1"
        />
        <test_BP
            BP_filter="filter"
            reagent_2_vol="acetic_anhydride_volume"
            reagent_1="glacial acetic acid"
            reagent_2="acetic anhydride"

        />
        <Dry
        vessel="filter"
        />
    </Procedure>

    </Synthesis>
</XDL>
