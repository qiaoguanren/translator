<Synthesis>

  <Hardware>
    <Component
      id="filter"
      type="ChemputerFilter"
    />
  </Hardware>

  <Reagents>
    <Reagent
      name="glacial acetic acid"
      density="1.05 g/mL"
    />
    <Reagent
      name="the moist solid iodinane oxide"
    />
    <Reagent
      name="acetone"
    />
    <Reagent
      name="Oxone deionized water solution"
      concentration="1.0 mol/L"
    />
    <Reagent
      name="2-Iodobenzoic acid"
      solid="True"
    />
    <Reagent
      name="anhydrous ether"
    />
    <Reagent
      name="acetic anhydride"
    />
    <Reagent
      name="water"
    />
  </Reagents>

  <Procedure>
    <Add
      vessel="filter"
      reagent="Oxone deionized water solution"
      amount="650.0 mmol"
      port="top"
    />
    <HeatChill
      vessel="filter"
      temp="70.0 °C"
      time="1200.0 secs"
      stir="true"
    />
    <HeatChill
      vessel="filter"
      temp="70.0 °C"
      time="10800.0 secs"
      stir="true"
    />
    <HeatChillToTemp
      vessel="filter"
      temp="5.0 °C"
      stir="true"
    />
    <HeatChill
      vessel="filter"
      temp="5.0 °C"
      time="5400.0 secs"
      stir="true"
      stir_speed="50 RPM"
    />
    <Filter
      vessel="filter"
    />
    <WashSolid
      vessel="filter"
      solvent="water"
      volume="100.0 mL"
      stir="solvent"
      repeats="6"
    />
    <WashSolid
      vessel="filter"
      solvent="acetone"
      volume="100.0 mL"
      stir="solvent"
      repeats="2"
    />
    <Dry
      vessel="filter"
      time="300.0 secs"
      temp="25.0 °C"
    />
    <Add
      vessel="filter"
      reagent="glacial acetic acid"
      amount="157.5 g"
      stir="True"
      port="top"
    />
    <Add
      vessel="filter"
      reagent="acetic anhydride"
      volume="300.0 mL"
      stir="True"
      port="top"
    />
    <HeatChill
      vessel="filter"
      temp="85.0 °C"
      time="1800.0 secs"
      stir="true"
    />
    <HeatChill
      vessel="filter"
      temp="85.0 °C"
      time="1200.0 secs"
      stir="true"
    />
    <HeatChill
      vessel="filter"
      temp="25.0 °C"
      time="86400.0 secs"
      stir="true"
    />
    <Filter
      vessel="filter"
    />
    <WashSolid
      vessel="filter"
      solvent="anhydrous ether"
      volume="80.0 mL"
      stir="solvent"
      repeats="3"
    />
    <Dry
      vessel="filter"
    />
  </Procedure>

</Synthesis>
