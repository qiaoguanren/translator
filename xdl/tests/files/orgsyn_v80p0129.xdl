<Synthesis>

  <Hardware>
    <Component
      id="filter"
      type="filter"
    />
  </Hardware>

  <Reagents>
    <Reagent
      name="copper(I)iodide"
      solid="true"
    />
    <Reagent
      name="distilled water"
    />
    <Reagent
      name="hydriodic acid"
    />
    <Reagent
      name="propiolic acid"
    />
  </Reagents>

  <Procedure>
    <AddSolid
      vessel="filter"
      reagent="copper(I)iodide"
      mass="0.2 g"
    />
    <Add
      vessel="filter"
      reagent="hydriodic acid"
      volume="40 mL"
    />
    <Add
      vessel="filter"
      reagent="propiolic acid"
      volume="10 mL"
      time="60 secs"
      stir="True"
    />
    <HeatChillToTemp
      vessel="filter"
      temp="130°C"
      stir="true"
    />
    <Stir
      vessel="filter"
      time="30 mins"
    />
    <HeatChillToTemp
      vessel="filter"
      temp="28°C"
      active="False"
      stir="true"
    />
    <HeatChill
      vessel="filter"
      temp="25°C"
      time="15 mins"
      stir="true"
    />
    <Filter
      vessel="filter"
    />
    <WashSolid
      vessel="filter"
      solvent="distilled water"
      volume="70 mL"
      repeats="3"
    />
    <Dry
      vessel="filter"
    />
  </Procedure>

</Synthesis>
