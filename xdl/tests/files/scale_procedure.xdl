<Synthesis>

  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
    <Component
      id="reactor"
      type="reactor"
    />
  </Hardware>

  <Reagents>
    <Reagent
      name="ether"
    />
    <Reagent
      name="water"
    />
  </Reagents>

  <Procedure>
    <Add
      vessel="reactor"
      reagent="ether"
      volume="20 mL"
    />
    <CleanVessel
      vessel="rotavap"
      solvent="water"
      volume="10 mL"
    />
  </Procedure>

</Synthesis>
