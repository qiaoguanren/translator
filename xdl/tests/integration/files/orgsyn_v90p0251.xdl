<?xdl version="0.2.0" ?>

<Synthesis filter_dead_volume_method="solvent">
  <Hardware>
    <Component
      id="cartridge_sodium sulfate"
      type="cartridge"
      chemical="sodium sulfate" />
    <Component
      id="filter"
      type="filter" />
    <Component
      id="reactor"
      type="reactor" />
    <Component
      id="rotavap"
      type="rotavap" />
    <Component
      id="separator"
      type="separator" />
    <Component
      id="phase_A"
      type="flask" />
    <Component
      id="phase_B"
      type="flask" />
  </Hardware>

  <Reagents>
    <Reagent
      id="2-aminophenethyl alcohol DME solution" />
    <Reagent
      id="MTBE" />
    <Reagent
      id="SOCl2" />
    <Reagent
      id="anhydrous DME" />
    <Reagent
      id="brine" />
    <Reagent
      id="ethyl acetate" />
    <Reagent
      id="sodium hydroxide" />
    <Reagent
      id="water" />
    <Reagent
      id="methanol" />
    <Reagent
      id="oxalic acid dihydrate" />
    <Reagent
      id="10 % methanol ethyl acetate solution" />
  </Reagents>

  <Procedure>
    <HeatChillToTemp
      vessel="reactor"
      temp="25°C" />
    <Add
      reagent="anhydrous DME"
      vessel="reactor"
      volume="80 mL"
      stir="False" />
    <Add
      reagent="SOCl2"
      vessel="reactor"
      volume="6.2 mL"
      stir="True" />
    <Add
      reagent="2-aminophenethyl alcohol DME solution"
      vessel="reactor"
      volume="20 mL"
      time="1.25 hrs"
      stir="True" />
    <Stir
      vessel="reactor"
      time="6.5 hrs" />
    <HeatChillToTemp
      vessel="reactor"
      temp="32°C" />
    <Add
      reagent="sodium hydroxide"
      vessel="reactor"
      volume="128 mL"
      time="30 mins"
      stir="True" />
    <Add
      reagent="water"
      vessel="reactor"
      volume="16 mL"
      time="30 mins"
      stir="True" />
    <HeatChillToTemp
      vessel="reactor"
      temp="60°C" />
    <Stir
      vessel="reactor"
      time="10 hrs" />
    <HeatChillToTemp
      vessel="reactor"
      temp="25°C" />
    <Transfer
      from_vessel="reactor"
      to_vessel="separator"
      volume="all"
      to_port="top" />
    <Add
      reagent="MTBE"
      vessel="separator"
      volume="100 mL"
      stir="True" />
    <Add
      reagent="water"
      vessel="separator"
      volume="56 mL"
      stir="True" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="phase_A"
      product_bottom="False"
      solvent=""
      n_separations="1"
      waste_phase_to_vessel="phase_B" />
    <Transfer
      from_vessel="phase_B"
      to_vessel="separator"
      volume="all"
      to_port="top" />
    <Separate
      purpose="extract"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="separator"
      product_bottom="False"
      solvent="MTBE"
      to_port="top"
      solvent_volume="56 mL"
      n_separations="1" />
    <Transfer
      from_vessel="phase_A"
      to_vessel="separator"
      volume="all"
      to_port="top" />
    <Separate
      purpose="wash"
      from_vessel="separator"
      separation_vessel="separator"
      to_vessel="rotavap"
      product_bottom="False"
      solvent="brine"
      through="sodium sulfate"
      to_port="evaporate"
      solvent_volume="43 mL"
      n_separations="1" />
    <Evaporate
      rotavap_name="rotavap"
      temp="35°C"
      pressure="79.9932 mbar"
      time="30 mins"
      mode="auto" />
    <Dissolve
      vessel="rotavap"
      solvent="ethyl acetate"
      volume="90 mL" />
    <Add
      reagent="oxalic acid dihydrate"
      vessel="filter"
      mass="9.3 g"
      stir="True" />
    <Add
      reagent="methanol"
      vessel="filter"
      volume="14 mL"
      stir="True" />
    <HeatChillToTemp
      vessel="filter"
      temp="25°C" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="filter"
      volume="30 mL"
      from_port="evaporate"
      to_port="top"
      dispense_speed="3" />
    <Stir
      vessel="filter"
      time="30 mins" />
    <Transfer
      from_vessel="rotavap"
      to_vessel="filter"
      volume="100 mL"
      from_port="evaporate"
      to_port="top"
      dispense_speed="0.83" />
    <HeatChill
      vessel="filter"
      temp="25°C"
      time="15 hrs" />
    <Filter
      filter_vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="10 % methanol ethyl acetate solution"
      volume="15 mL"
      repeat="2" />
    <Dry
      vessel="filter" />
  </Procedure>

</Synthesis>
