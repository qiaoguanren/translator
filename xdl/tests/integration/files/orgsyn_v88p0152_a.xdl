<?xdl version="0.1.10" ?>

<Synthesis>
  <Hardware>
    <Component
      id="filter"
      type="filter" />
    <Component
      id="flask_washings"
      type="flask" />
  </Hardware>

  <Reagents>
    <Reagent
      id="anhydrous Et2O" />
    <Reagent
      id="benzyl carbamate" />
    <Reagent
      id="glyoxylic acid monohydrate" />
    <Reagent
      id="hexanes-Et2O" />
  </Reagents>

  <Procedure>
    <Add
      reagent="benzyl carbamate"
      vessel="filter"
      mass="30.23 g"
      stir="False" />
    <Add
      reagent="glyoxylic acid monohydrate"
      vessel="filter"
      mass="20.25 g"
      stir="False" />
    <Evacuate
      vessel="filter" />
    <Add
      reagent="anhydrous Et2O"
      vessel="filter"
      volume="200 mL"
      stir="False" />
    <HeatChill
      vessel="filter"
      temp="34.5°C"
      time="12 hrs"
      stir_speed="200.0" />
    <HeatChillToTemp
      vessel="filter"
      temp="25°C"
      active="True"
      continue_heatchill="False" />
    <Filter
      filter_vessel="filter"
      filtrate_vessel="flask_washings" />
    <WashSolid
      vessel="filter"
      solvent="hexanes-Et2O"
      volume="20 mL"
      repeat="6"
      filtrate_vessel="flask_washings" />
    <Dry
      vessel="filter" />
  </Procedure>

</Synthesis>
