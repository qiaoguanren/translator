<Synthesis organic_cleaning_solvent="anhydrous ether">
  <Hardware>
    <Component
      id="filter"
      type="ChemputerFilter" />
  </Hardware>

  <Reagents>
    <Reagent
      id="glacial acetic acid" />
    <Reagent
      id="the moist solid iodinane oxide" />
    <Reagent
      id="acetone" />
    <Reagent
      id="Oxone deionized water solution" />
    <Reagent
      id="2-Iodobenzoic acid" />
    <Reagent
      id="anhydrous ether" />
    <Reagent
      id="acetic anhydride" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="Oxone deionized water solution"
      vessel="filter"
      volume="650.0"
      port="top"
      stir="False" />
    <!-- <Add
      reagent="2-Iodobenzoic acid"
      vessel="filter"
      mass="50.0"
      port="top"
      stir="True" /> -->
    <HeatChill
      vessel="filter"
      temp="70.0"
      time="1200.0" />
    <HeatChill
      vessel="filter"
      temp="70.0"
      time="10800.0" />
    <HeatChillToTemp
      vessel="filter"
      temp="5.0" />
    <HeatChill
      vessel="filter"
      temp="5.0"
      time="5400.0"
      stir_speed="50" />
    <Filter
      filter_vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="water"
      volume="100.0"
      stir="solvent"
      repeat="6" />
    <WashSolid
      vessel="filter"
      solvent="acetone"
      volume="100.0"
      stir="solvent"
      repeat="2" />
    <Dry
      vessel="filter"
      time="300.0"
      temp="25.0" />
    <!-- <Add
      reagent="the moist solid iodinane oxide"
      vessel="filter"
      mass="88.2"
      port="top"
      stir="False" /> -->
    <Add
      reagent="glacial acetic acid"
      vessel="filter"
      volume="150.0"
      port="top"
      stir="True" />
    <Add
      reagent="acetic anhydride"
      vessel="filter"
      volume="300.0"
      port="top"
      stir="True" />
    <HeatChill
      vessel="filter"
      temp="85.0"
      time="1800.0" />
    <HeatChill
      vessel="filter"
      temp="85.0"
      time="1200.0" />
    <HeatChill
      vessel="filter"
      temp="25.0"
      time="86400.0" />
    <Filter
      filter_vessel="filter" />
    <WashSolid
      vessel="filter"
      solvent="anhydrous ether"
      volume="80.0"
      stir="solvent"
      repeat="3" />
    <Dry
      vessel="filter" />
  </Procedure>

</Synthesis>
