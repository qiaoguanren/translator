<Synthesis auto_clean="false">
  <Hardware>
    <Component type="filter" id="filter" />
  </Hardware>

  <Reagents>
    <Reagent id="ether" />
  </Reagents>

  <Procedure>
    <Repeat repeats="2">
      <Wait time="5" />
      <Wait time="10" />
      <Add
      vessel="filter"
      reagent="ether"
      volume="20 mL" />
    </Repeat>
  </Procedure>

</Synthesis>

